<config>
  <!-- set some datasource defaults -->

  <param name="datasource.username">RICE</param>
  <param name="datasource.password">RICE</param>

  <!-- Oracle example -->
  <param name="datasource.ojb.platform">Oracle9i</param>
  <param name="datasource.url">jdbc:oracle:thin:@localhost:1521:XE</param>
  <param name="datasource.platform">org.kuali.rice.core.framework.persistence.platform.OracleDatabasePlatform</param>
  <param name="datasource.driver.name">oracle.jdbc.driver.OracleDriver</param>
  <param name="encryption.key"></param>

  <!-- Conversion Job Settings - OPTIONAL, if they are not here user will be prompted for the values -->

  <!--
     Run mode options :
    1. Update xml in DB
    2. Print old and new xml
  -->
  <param name="runMode">2</param>


  <!--
     fromRange options:
    1. all for all documents
    2. Starting document number such as 1000
  -->
  <param name="fromRange">all</param>
  <!--<param name="fromRange">1000</param>-->


  <!--
     toRange options:  (This is not needed if all is used for fromRange
    1. Ending document number such as 60000
  -->
  <!--<param name="toRange">60000</param>-->

</config>
