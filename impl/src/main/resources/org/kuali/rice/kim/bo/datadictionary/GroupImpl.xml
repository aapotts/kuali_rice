  <bean id="GroupImpl" parent="GroupImpl-parentBean"/>

  <bean id="GroupImpl-parentBean" abstract="true" parent="BusinessObjectEntry" p:businessObjectClass="org.kuali.rice.kim.bo.impl.GroupImpl" p:titleAttribute="groupId" p:objectLabel="Group" p:inquiryDefinition-ref="GroupImpl-inquiryDefinition" p:lookupDefinition-ref="GroupImpl-lookupDefinition">
	<property name="exporterClass" value="org.kuali.rice.kew.export.DataExporter"/> 
    <property name="attributes">
      <list>
        <ref bean="GroupImpl-groupId"/>
        <ref bean="GroupImpl-groupName"/>
        <ref bean="GroupImpl-namespaceCode"/>
        <ref bean="GroupImpl-kimTypeId"/>
        <ref bean="GroupImpl-kimTypeName"/>
        <ref bean="GroupImpl-kimTypeNamespaceCode"/>
        <ref bean="GroupImpl-active"/>
        <ref bean="GroupImpl-groupDescription"/>
      </list>
    </property>
    <property name="collections">
      <list>
        <bean parent="CollectionDefinition" p:name="members" p:label="Assigned Principals" p:shortLabel="Assigned Principals" p:summary="Assigned Principals" p:description="Assigned Principals"/>
        <bean parent="CollectionDefinition" p:name="memberPersons" p:label="Assigned Principals" p:shortLabel="Assigned Principals" p:summary="Assigned Principals" p:description="Assigned Principals"/>
        <bean parent="CollectionDefinition" p:name="memberGroups" p:label="Assigned Groups" p:shortLabel="Assigned Groups" p:summary="Assigned Groups" p:description="Assigned Groups"/>
      </list>
    </property>
    <property name="relationships">
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="kimTypeInfo"/>
          <property name="primitiveAttributes">
            <list>
              <bean parent="PrimitiveAttributeDefinition" p:sourceName="kimTypeId" p:targetName="id"/>
            </list>
          </property>
          <property name="supportAttributes">
            <list>
              <bean parent="SupportAttributeDefinition" p:identifier="true" p:sourceName="kimTypeInfo.name" p:targetName="name"/>
              <bean parent="SupportAttributeDefinition" p:identifier="true" p:sourceName="kimTypeInfo.namespaceCode" p:targetName="namespaceCode"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

  <bean id="GroupImpl-groupId" parent="GroupImpl-groupId-parentBean"/>
  <bean id="GroupImpl-groupId-parentBean" parent="AttributeDefinition" abstract="true" p:name="groupId" p:forceUppercase="true" p:label="Group Id" p:shortLabel="Group Id" p:maxLength="70" p:required="false">
    <property name="control">
      <bean parent="TextControlDefinition" p:size="40"/>
    </property>
  </bean>

  <bean id="GroupImpl-kimTypeId" parent="GroupImpl-kimTypeId-parentBean"/>
  <bean id="GroupImpl-kimTypeId-parentBean" parent="AttributeDefinition" abstract="true" p:name="kimTypeId" p:forceUppercase="true" p:label="Group Type" p:shortLabel="Group Type" p:maxLength="40" p:required="false">
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
  </bean>

  <bean id="GroupImpl-kimTypeName" parent="GroupImpl-kimTypeName-parentBean"/>
  <bean id="GroupImpl-kimTypeName-parentBean" abstract="true" parent="KimType-name">
    <property name="name" value="kimTypeInfo.name"/>
    <property name="label" value="Group Type Name"/>
  </bean>

  <bean id="GroupImpl-kimTypeNamespaceCode" parent="GroupImpl-kimTypeNamespaceCode-parentBean"/>
  <bean id="GroupImpl-kimTypeNamespaceCode-parentBean" abstract="true" parent="KimType-namespaceCode">
    <property name="name" value="kimTypeInfo.namespaceCode"/>
    <property name="label" value="Group Type Name Space"/>
  </bean>

  <bean id="GroupImpl-groupName" parent="GroupImpl-groupName-parentBean"/>
  <bean id="GroupImpl-groupName-parentBean" parent="AttributeDefinition" abstract="true" p:name="groupName" p:forceUppercase="false" p:label="Group Name" p:shortLabel="Group Name" p:maxLength="80" p:required="true">
    <property name="control">
      <bean parent="TextControlDefinition" p:size="50"/>
    </property>
  </bean>

	<!-- Added Group Description -->
  <bean id="GroupImpl-groupDescription" parent="GroupImpl-groupDescription-parentBean"/>
  <bean id="GroupImpl-groupDescription-parentBean" parent="AttributeDefinition" abstract="true" p:name="groupDescription" p:forceUppercase="false" p:label="Group Description" p:shortLabel="Group Description" p:maxLength="4000" p:required="false">
    <property name="control">
      <bean parent="TextControlDefinition" p:size="50"/>
    </property>
  </bean>

  <bean id="GroupImpl-namespaceCode" parent="GroupImpl-namespaceCode-parentBean"/>
  <bean id="GroupImpl-namespaceCode-parentBean" parent="KimType-namespaceCode" abstract="true" p:name="namespaceCode" p:label="Group Namespace" p:shortLabel="Group Namespace" p:required="true" p:maxLength="40">
  </bean>

  <bean id="GroupImpl-active" parent="GroupImpl-active-parentBean"/>
  <bean id="GroupImpl-active-parentBean" parent="KimType-active" abstract="true" p:label="Active?"/>

  <bean id="GroupImpl-inquiryDefinition" parent="GroupImpl-inquiryDefinition-parentBean"/>

  <bean id="GroupImpl-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Group"/>
    <property name="inquirableClass" value="org.kuali.rice.kim.inquiry.GroupInquirableImpl"/>
    <property name="inquirySections">
      <list>
        <bean parent="InquirySectionDefinition" p:title="Group" p:numberOfColumns="1">
          <property name="inquiryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="groupId"/>
              <bean parent="FieldDefinition" p:attributeName="namespaceCode"/>
              <bean parent="FieldDefinition" p:attributeName="groupName"/>
              <bean parent="FieldDefinition" p:attributeName="groupDescription"/>
              <!-- <bean parent="FieldDefinition" p:attributeName="kimTypeId" />-->
              <bean parent="FieldDefinition" p:attributeName="kimTypeInfo.namespaceCode" p:noInquiry="true"/>
		      <bean parent="FieldDefinition" p:attributeName="kimTypeInfo.name"/>
            </list>
          </property>
        </bean>
		<bean parent="InquirySectionDefinition">
          <property name="title" value="Assigned Groups"/>
          <property name="numberOfColumns" value="1"/>
          <property name="inquiryFields">
            <list>
              <bean parent="InquiryCollectionDefinition">
                <property name="attributeName" value="memberGroups"/>
                <property name="businessObjectClass" value="org.kuali.rice.kim.bo.impl.GroupImpl"/>
                <property name="numberOfColumns" value="1"/>
                <property name="inquiryFields">
                  <list>
                      <bean parent="FieldDefinition" p:attributeName="groupId"/>
				      <bean parent="FieldDefinition" p:attributeName="namespaceCode"/>
				      <bean parent="FieldDefinition" p:attributeName="groupName"/>
				      <bean parent="FieldDefinition" p:attributeName="groupDescription"/>
				      <bean parent="FieldDefinition" p:attributeName="kimTypeInfo.namespaceCode" p:noInquiry="true"/>
				      <bean parent="FieldDefinition" p:attributeName="kimTypeInfo.name"/>
                  </list>
                </property>
                <property name="summaryFields">
                  <list>
                      <bean parent="FieldDefinition" p:attributeName="namespaceCode"/>
				      <bean parent="FieldDefinition" p:attributeName="groupName"/>
				      <bean parent="FieldDefinition" p:attributeName="groupDescription"/>
                  </list>
                </property>
              </bean>
            </list>
          </property>
        </bean>

        <bean parent="InquirySectionDefinition">
          <property name="title" value="Assigned Principals"/>
          <property name="numberOfColumns" value="1"/>
          <property name="inquiryFields">
            <list>
              <bean parent="InquiryCollectionDefinition">
                <property name="attributeName" value="memberPersons"/>
                <property name="businessObjectClass" value="org.kuali.rice.kim.bo.impl.PersonImpl"/>
                <property name="numberOfColumns" value="1"/>
                <property name="inquiryFields">
                  <list>
                      <bean parent="FieldDefinition" p:attributeName="principalName"/>
                      <bean parent="FieldDefinition" p:attributeName="name"/>
		              <bean parent="FieldDefinition" p:attributeName="firstName"/>
		              <bean parent="FieldDefinition" p:attributeName="middleName"/>
		              <bean parent="FieldDefinition" p:attributeName="lastName"/>
		              <bean parent="FieldDefinition" p:attributeName="principalId"/>
                  </list>
                </property>
                <property name="summaryFields">
                  <list>
                      <bean parent="FieldDefinition" p:attributeName="principalName"/>
                  </list>
                </property>
              </bean>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

  <bean id="GroupImpl-lookupDefinition" parent="GroupImpl-lookupDefinition-parentBean"/>

  <bean id="GroupImpl-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition" p:title="Group Lookup">
    <property name="lookupableID" value="groupLookupable"/>

    <property name="defaultSort">
      <bean parent="SortDefinition">
        <property name="attributeNames">
          <list>
            <value>groupId</value>
          </list>
        </property>
        <property name="sortAscending" value="true"/>
      </bean>
    </property>
    <property name="lookupFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="groupId"/>
        <bean parent="FieldDefinition" p:attributeName="namespaceCode"/>
        <bean parent="FieldDefinition" p:attributeName="groupName"/>
        <bean parent="FieldDefinition" p:attributeName="groupDescription"/>
        <bean parent="FieldDefinition" p:attributeName="kimTypeId"/>
        <bean parent="FieldDefinition" p:attributeName="active" p:defaultValue="Y"/>
        <!--
             Note that kimTypeId and PrincipalName fields
             are overwritten/created in GroupLookupableHelperServiceImpl
        -->
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="kimTypeInfo.name"/>
        <bean parent="FieldDefinition" p:attributeName="namespaceCode"/>
        <bean parent="FieldDefinition" p:attributeName="groupName"/>
        <bean parent="FieldDefinition" p:attributeName="groupDescription"/>
      </list>
    </property>
  </bean>

</beans>
