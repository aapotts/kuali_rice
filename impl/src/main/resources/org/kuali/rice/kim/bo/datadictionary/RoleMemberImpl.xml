<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="RoleMemberImpl" parent="RoleMemberImpl-parentBean"/>

  <bean id="RoleMemberImpl-parentBean" abstract="true" parent="BusinessObjectEntry" p:businessObjectClass="org.kuali.rice.kim.bo.role.impl.RoleMemberImpl" p:titleAttribute="roleMemberId" p:objectLabel="Names" p:inquiryDefinition-ref="RoleMemberImpl-inquiryDefinition" p:lookupDefinition-ref="RoleMemberImpl-lookupDefinition">
    <property name="attributes">
      <list>
        <ref bean="RoleMemberImpl-roleId"/>
        <ref bean="RoleMemberImpl-roleMemberId"/>
        <ref bean="RoleMemberImpl-memberId"/>
        <!-- TODO: create entry for this  -->
        <ref bean="RoleMemberImpl-memberTypeCode"/>
        <ref bean="RoleMemberImpl-activeFromDate"/>
        <ref bean="RoleMemberImpl-activeToDate"/>
      </list>
    </property>
    
  </bean>
 
  <bean id="RoleMemberImpl-roleId" parent="RoleMemberImpl-roleId-parentBean"/>
  <bean id="RoleMemberImpl-roleId-parentBean" parent="RoleImpl-roleId"/>
 
  <bean id="RoleMemberImpl-roleMemberId" parent="RoleMemberImpl-roleMemberId-parentBean"/>
  <bean id="RoleMemberImpl-roleMemberId-parentBean" parent="AttributeDefinition" abstract="true" p:name="roleMemberId" p:forceUppercase="true" p:label="Role Member Identifier" p:shortLabel="Role Member Identifier" p:maxLength="40" p:required="false">
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
  </bean>
  
  <bean id="RoleMemberImpl-memberId" parent="RoleMemberImpl-memberId-parentBean"/>
  <bean id="RoleMemberImpl-memberId-parentBean" parent="AttributeDefinition" abstract="true" p:name="memberId" p:forceUppercase="true" p:label="Member Identifier" p:shortLabel="Member Identifier" p:maxLength="40" p:required="false">
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
  </bean>
    
  <bean id="RoleMemberImpl-memberTypeCode" parent="RoleMemberImpl-memberTypeCode-parentBean"/>
  <bean id="RoleMemberImpl-memberTypeCode-parentBean" parent="AttributeDefinition" abstract="true" p:name="memberTypeCode" p:forceUppercase="true" p:label="Member Type Code" p:shortLabel="Type Code" p:maxLength="40" p:required="false">
    <property name="control">
      <bean parent="SelectControlDefinition" p:valuesFinderClass="org.kuali.rice.kim.bo.options.MemberTypeValuesFinder"/>
    </property>
  </bean>
            
  <bean id="RoleMemberImpl-activeFromDate" parent="RoleMemberImpl-activeFromDate-parentBean"/>
  <bean id="RoleMemberImpl-activeFromDate-parentBean" abstract="true" parent="AttributeReferenceDummy-activeFromDate">
    <property name="name" value="activeFromDate"/>
    <property name="label" value="Active From Date"/>
    <property name="shortLabel" value="Active From Dt"/>
    <property name="required" value="false"/>
    <property name="summary" value="Start Date"/>
    <property name="description" value="Start Date"/>
  </bean>

  <bean id="RoleMemberImpl-activeToDate" parent="RoleMemberImpl-activeToDate-parentBean"/>
  <bean id="RoleMemberImpl-activeToDate-parentBean" abstract="true" parent="AttributeReferenceDummy-activeToDate">
    <property name="name" value="activeToDate"/>
    <property name="label" value="Active To Date"/>
    <property name="shortLabel" value="Active To Dt"/>
    <property name="required" value="false"/>
    <property name="summary" value="End Date"/>
    <property name="description" value="End Date"/>
  </bean>

  <bean id="RoleMemberImpl-inquiryDefinition" parent="RoleMemberImpl-inquiryDefinition-parentBean"/>

  <bean id="RoleMemberImpl-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition" p:title="Person Group Inquiry">
    <property name="inquirySections">
      <list>
        <bean parent="InquirySectionDefinition" p:title="Person Attributes" p:numberOfColumns="1">
          <property name="inquiryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="roleMemberId"/>
              <bean parent="FieldDefinition" p:attributeName="memberId"/>
              <bean parent="FieldDefinition" p:attributeName="activeFromDate"/>
              <bean parent="FieldDefinition" p:attributeName="activeToDate"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

  <bean id="RoleMemberImpl-lookupDefinition" parent="RoleMemberImpl-lookupDefinition-parentBean"/>

  <bean id="RoleMemberImpl-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition" p:title="Role Member Lookup">
    <property name="lookupableID" value="roleMemberImplLookupable"/>
    <property name="defaultSort">
      <bean parent="SortDefinition">
        <property name="attributeNames">
          <list>
            <value>roleMemberId</value>
          </list>
        </property>
        <property name="sortAscending" value="true"/>
      </bean>
    </property>
    <property name="lookupFields">
      <list>
              <bean parent="FieldDefinition" p:attributeName="roleId" p:readOnly="true"/>
              <bean parent="FieldDefinition" p:attributeName="roleMemberId"/>
              <bean parent="FieldDefinition" p:attributeName="memberId"/>
              <bean parent="FieldDefinition" p:attributeName="memberTypeCode"/>
              <bean parent="FieldDefinition" p:attributeName="activeFromDate"/>
              <bean parent="FieldDefinition" p:attributeName="activeToDate"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
              <bean parent="FieldDefinition" p:attributeName="roleId"/>
              <bean parent="FieldDefinition" p:attributeName="roleMemberId"/>
              <bean parent="FieldDefinition" p:attributeName="memberId"/>
              <bean parent="FieldDefinition" p:attributeName="memberTypeCode"/>
              <bean parent="FieldDefinition" p:attributeName="activeFromDate"/>
              <bean parent="FieldDefinition" p:attributeName="activeToDate"/>
      </list>
    </property>
  </bean>

</beans>
