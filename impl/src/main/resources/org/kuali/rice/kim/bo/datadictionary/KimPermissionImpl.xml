  <bean id="KimPermissionImpl" parent="KimPermissionImpl-parentBean"/>

  <bean id="KimPermissionImpl-parentBean" abstract="true" parent="BusinessObjectEntry" p:businessObjectClass="org.kuali.rice.kim.bo.role.impl.KimPermissionImpl" p:titleAttribute="permissionId" p:objectLabel="Permission" p:inquiryDefinition-ref="KimPermissionImpl-inquiryDefinition">
    <property name="attributes">
      <list>
        <ref bean="KimPermissionImpl-template.namespaceCode"/>
        <ref bean="KimPermissionImpl-template.name"/>
        <ref bean="KimPermissionImpl-namespaceCode"/>
        <ref bean="KimPermissionImpl-name"/>
        <ref bean="KimPermissionImpl-description"/>
        <ref bean="KimPermissionImpl-detailObjects"/>
        <ref bean="KimPermissionImpl-templateId"/>
        <ref bean="KimPermissionImpl-detailObjectsValues"/>
      </list>
    </property>
	<property name="relationships">
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="template"/>
          <property name="primitiveAttributes">
            <list>
              <bean parent="PrimitiveAttributeDefinition" p:sourceName="templateId" p:targetName="permissionTemplateId"/>
            </list>
          </property>
          <property name="supportAttributes">
            <list>
              <bean parent="SupportAttributeDefinition" p:sourceName="template.namespaceCode" p:targetName="namespaceCode"/>
              <bean parent="SupportAttributeDefinition" p:sourceName="template.name" p:targetName="name"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

  <bean id="KimPermissionImpl-template.namespaceCode" parent="KimPermissionImpl-template.namespaceCode-parentBean"/>
  <bean id="KimPermissionImpl-template.namespaceCode-parentBean" parent="Parameter-namespaceCode" abstract="true" p:name="template.namespaceCode" p:label="Template Namespace" p:shortLabel="Template Namespace">
  </bean>
    
  <bean id="KimPermissionImpl-templateId" parent="KimPermissionImpl-templateId-parentBean"/>
  <bean id="KimPermissionImpl-templateId-parentBean" parent="AttributeDefinition" abstract="true" p:name="templateId" p:label="Template Id" p:shortLabel="Template Id" p:maxLength="40" p:required="false">
    <property name="control">
      <bean parent="TextControlDefinition" p:size="40"/>
    </property>
  </bean>
    
  <bean id="KimPermissionImpl-template.name" parent="KimPermissionImpl-template.name-parentBean"/>
  <bean id="KimPermissionImpl-template.name-parentBean" parent="AttributeDefinition" abstract="true" p:name="template.name" p:label="Template Name" p:shortLabel="Template Name" p:maxLength="40" p:required="false">
    <property name="control">
      <bean parent="TextControlDefinition" p:size="40"/>
    </property>
  </bean>
  
  
  <bean id="KimPermissionImpl-namespaceCode" parent="KimPermissionImpl-namespaceCode-parentBean"/>
  <bean id="KimPermissionImpl-namespaceCode-parentBean" parent="Parameter-namespaceCode" abstract="true" p:name="namespaceCode" p:label="Permission Namespace" p:shortLabel="Permission Namespace">
  </bean>
  
  <bean id="KimPermissionImpl-name" parent="KimPermissionImpl-name-parentBean"/>
  <bean id="KimPermissionImpl-name-parentBean" abstract="true" parent="KimTypeImpl-name" p:name="name" p:label="Permission Name" p:shortLabel="Permission Name">
    <property name="control">
      <bean parent="TextControlDefinition" p:size="40"/>
    </property>
  </bean>

  <bean id="KimPermissionImpl-description" parent="KimPermissionImpl-description-parentBean"/>
  <bean id="KimPermissionImpl-description-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false"/>
    <property name="shortLabel" value="Desc"/>
    <property name="maxLength" value="400"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true" p:maxLength="400"/>
    </property>
    <property name="control">
      <bean parent="TextareaControlDefinition" p:cols="40" p:rows="3"/>
    </property>
    <property name="name" value="description"/>
    <property name="label" value="Permission Description"/>
  </bean>

  <bean id="KimPermissionImpl-detailObjects" parent="KimPermissionImpl-detailObjects-parentBean"/>
  <bean id="KimPermissionImpl-detailObjects-parentBean" parent="AttributeDefinition" abstract="true" p:name="detailObjects" p:label="Permission Detail Values" p:shortLabel="Detail Values" p:maxLength="40" p:required="false">
    <property name="control">
      <bean parent="TextControlDefinition" p:size="40"/>
    </property>
  </bean>

  <bean id="KimPermissionImpl-detailObjectsValues" parent="KimPermissionImpl-detailObjects" p:name="detailObjectsValues">
  </bean>

  <bean id="KimPermissionImpl-inquiryDefinition" parent="KimPermissionImpl-inquiryDefinition-parentBean"/>
  <bean id="KimPermissionImpl-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition" p:title="Permission Inquiry">
    <property name="inquirySections">
      <list>
        <bean parent="InquirySectionDefinition" p:title="Permission Attributes" p:numberOfColumns="1">
          <property name="inquiryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="template.namespaceCode"/>
              <bean parent="FieldDefinition" p:attributeName="template.name"/>
        	  <bean parent="FieldDefinition" p:attributeName="namespaceCode"/>
        	  <bean parent="FieldDefinition" p:attributeName="name"/>
	       	  <bean parent="FieldDefinition" p:attributeName="detailObjectsValues"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
    <property name="inquirableClass" value="org.kuali.rice.kim.inquiry.PermissionInquirableImpl"/>
  </bean>

</beans>
