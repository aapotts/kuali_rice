  <bean id="KimPermissionTemplateImpl" parent="KimPermissionTemplateImpl-parentBean"/>

  <bean id="KimPermissionTemplateImpl-parentBean" abstract="true" parent="BusinessObjectEntry" p:businessObjectClass="org.kuali.rice.kim.bo.role.impl.KimPermissionTemplateImpl" p:titleAttribute="permissionTemplateId" p:objectLabel="Permission Templates" p:inquiryDefinition-ref="KimPermissionTemplateImpl-inquiryDefinition" p:lookupDefinition-ref="KimPermissionTemplateImpl-lookupDefinition">
    <property name="attributes">
      <list>
        <ref bean="KimPermissionTemplateImpl-templateId"/>
        <ref bean="KimPermissionTemplateImpl-namespaceCode"/>
        <ref bean="KimPermissionTemplateImpl-name"/>
        <ref bean="KimPermissionTemplateImpl-description"/>
      </list>
    </property>
  </bean>

  <bean id="KimPermissionTemplateImpl-templateId" parent="KimPermissionTemplateImpl-templateId-parentBean"/>
  <bean id="KimPermissionTemplateImpl-templateId-parentBean" parent="AttributeDefinition" abstract="true" p:name="permissionTemplateId" p:forceUppercase="true" p:label="Template Id" p:shortLabel="Template Id" p:maxLength="40" p:required="false">
    <property name="control">
      <bean parent="TextControlDefinition" p:size="40"/>
    </property>
  </bean>

  <bean id="KimPermissionTemplateImpl-namespaceCode" parent="KimPermissionTemplateImpl-namespaceCode-parentBean"/>
  <bean id="KimPermissionTemplateImpl-namespaceCode-parentBean" parent="RoleBo-namespaceCode" abstract="true" p:name="namespaceCode" p:label="Template Namespace" p:shortLabel="Template Namespace">
  </bean>
    
  <bean id="KimPermissionTemplateImpl-name" parent="KimPermissionTemplateImpl-name-parentBean"/>
  <bean id="KimPermissionTemplateImpl-name-parentBean" parent="AttributeDefinition" abstract="true" p:name="name" p:label="Template Name" p:shortLabel="Template Name" p:maxLength="40" p:required="false">
    <property name="control">
      <bean parent="TextControlDefinition" p:size="40"/>
    </property>
  </bean>

  <bean id="KimPermissionTemplateImpl-description" parent="KimPermissionTemplateImpl-description-parentBean"/>
  <bean id="KimPermissionTemplateImpl-description-parentBean" parent="AttributeDefinition" abstract="true" p:name="description" p:forceUppercase="true" p:label="Permission Template Description" p:shortLabel="Perm Templ Description" p:maxLength="40" p:required="false">
    <property name="control">
      <bean parent="TextControlDefinition" p:size="40"/>
    </property>
  </bean>

  <bean id="KimPermissionTemplateImpl-inquiryDefinition" parent="KimPermissionTemplateImpl-inquiryDefinition-parentBean"/>
  <bean id="KimPermissionTemplateImpl-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition" p:title="Permission Template Inquiry">
    <property name="inquirySections">
      <list>
        <bean parent="InquirySectionDefinition" p:title="Permission Template Attributes" p:numberOfColumns="1">
          <property name="inquiryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="namespaceCode"/>
              <bean parent="FieldDefinition" p:attributeName="name"/>
        	  <bean parent="FieldDefinition" p:attributeName="description"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

  <bean id="KimPermissionTemplateImpl-lookupDefinition" parent="KimPermissionTemplateImpl-lookupDefinition-parentBean"/>
  <bean id="KimPermissionTemplateImpl-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition" p:title="Permission Template Lookup">
    <property name="defaultSort">
      <bean parent="SortDefinition">
        <property name="attributeNames">
          <list>
            <value>permissionTemplateId</value>
          </list>
        </property>
        <property name="sortAscending" value="true"/>
      </bean>
    </property>
    <property name="lookupFields">
      <list>
          <bean parent="FieldDefinition" p:attributeName="namespaceCode"/>
          <bean parent="FieldDefinition" p:attributeName="name"/>
       	  <bean parent="FieldDefinition" p:attributeName="description"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
          <bean parent="FieldDefinition" p:attributeName="permissionTemplateId"/>
          <bean parent="FieldDefinition" p:attributeName="namespaceCode"/>
          <bean parent="FieldDefinition" p:attributeName="name"/>
       	  <bean parent="FieldDefinition" p:attributeName="description"/>
      </list>
    </property>
  </bean>

</beans>
