  <bean id="DocName" parent="DocName-parentBean"/>

  <bean id="DocName-parentBean" abstract="true" parent="BusinessObjectEntry" p:businessObjectClass="org.kuali.rice.kim.bo.ui.PersonDocumentName" p:titleAttribute="nameTypeCode" p:objectLabel="Names" p:inquiryDefinition-ref="DocName-inquiryDefinition" p:lookupDefinition-ref="DocName-lookupDefinition">
    <property name="attributes">
      <list>
        <ref bean="DocName-firstName"/>
        <ref bean="DocName-lastName"/>
        <ref bean="DocName-suffix"/>
        <ref bean="DocName-title"/>
        <ref bean="DocName-nameTypeCode"/>
        <ref bean="DocName-active"/>
        <ref bean="DocName-dflt"/>
      </list>
    </property>
  </bean>

  <bean id="DocName-firstName" parent="DocName-firstName-parentBean"/>
  <bean id="DocName-firstName-parentBean" parent="AttributeDefinition" abstract="true" p:name="firstName" p:forceUppercase="true" p:label="First Name" p:shortLabel="First Name" p:maxLength="40" p:required="false">
    <property name="validationPattern">

      <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true" p:maxLength="40" />

    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
  </bean>

  <bean id="DocName-lastName" parent="DocName-lastName-parentBean"/>
  <bean id="DocName-lastName-parentBean" parent="AttributeDefinition" abstract="true" p:name="lastName" p:forceUppercase="true" p:label="Last Name" p:shortLabel="Last Name" p:maxLength="80" p:required="false">
    <property name="validationPattern">

      <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true" p:maxLength="80" />

    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
  </bean>

  <bean id="DocName-suffix" parent="DocName-suffix-parentBean"/>
  <bean id="DocName-suffix-parentBean" parent="AttributeDefinition" abstract="true" p:name="suffix" p:forceUppercase="true" p:label="Suffix" p:shortLabel="Suffix" p:maxLength="20" p:required="false">
    <property name="control">
      <bean parent="SelectControlDefinition" p:businessObjectClass="org.kuali.rice.core.impl.parameter.ParameterBo" p:valuesFinderClass="org.kuali.rice.kim.bo.options.NameSuffixValuesFinder" p:includeKeyInLabel="false" p:keyAttribute="namespaceCode" p:labelAttribute="name"/>
    </property>
  </bean>
 
   <bean id="DocName-nameTypeCode" parent="DocName-nameTypeCode-parentBean"/>
  <bean id="DocName-nameTypeCode-parentBean" parent="AttributeDefinition" abstract="true" p:name="nameTypeCode" p:forceUppercase="true" p:label="Name Type" p:shortLabel="Name Type" p:maxLength="40" p:required="false">
    <property name="control">
      <bean parent="SelectControlDefinition" p:businessObjectClass="org.kuali.rice.kim.impl.identity.name.EntityNameTypeBo" p:valuesFinderClass="org.kuali.rice.krad.lookup.keyvalues.PersistableBusinessObjectValuesFinder" p:includeKeyInLabel="false" p:keyAttribute="code" p:labelAttribute="name"/>
    </property>
  </bean>
    
  <bean id="DocName-title" parent="DocName-title-parentBean"/>
  <bean id="DocName-title-parentBean" parent="AttributeDefinition" abstract="true" p:name="title" p:forceUppercase="true" p:label="Title" p:shortLabel="Title" p:maxLength="20" p:required="false">
    <property name="control">
      <bean parent="SelectControlDefinition" p:businessObjectClass="org.kuali.rice.core.impl.parameter.ParameterBo" p:valuesFinderClass="org.kuali.rice.kim.bo.options.NameTitleValuesFinder" p:includeKeyInLabel="false" p:keyAttribute="namespaceCode" p:labelAttribute="name"/>
    </property>
  </bean>
  
     <bean id="DocName-active" parent="DocName-active-parentBean"/>
  <bean id="DocName-active-parentBean" abstract="true" parent="AttributeReferenceDummy-genericBoolean">
    <property name="name" value="active"/>
    <property name="label" value="Active"/>
    <property name="shortLabel" value="Active"/>
    <property name="required" value="false"/>
    <property name="summary" value="Active"/>
    <property name="description" value="Active"/>
  </bean>
 
  <bean id="DocName-dflt" parent="DocName-dflt-parentBean"/>
  <bean id="DocName-dflt-parentBean" abstract="true" parent="AttributeReferenceDummy-genericBoolean">
    <property name="name" value="dflt"/>
    <property name="label" value="Default"/>
    <property name="shortLabel" value="Default"/>
    <property name="required" value="false"/>
    <property name="summary" value="Default"/>
    <property name="description" value="Default"/>
  </bean>

  
  
  <bean id="DocName-inquiryDefinition" parent="DocName-inquiryDefinition-parentBean"/>

  <bean id="DocName-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition" p:title="Person Name Inquiry">
    <property name="inquirySections">
      <list>
        <bean parent="InquirySectionDefinition" p:title="Person Attributes" p:numberOfColumns="1">
          <property name="inquiryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="nameTypeCode"/>
              <bean parent="FieldDefinition" p:attributeName="title"/>
              <bean parent="FieldDefinition" p:attributeName="firstName"/>
        	  <bean parent="FieldDefinition" p:attributeName="lastName"/>
              <bean parent="FieldDefinition" p:attributeName="suffix"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

  <bean id="DocName-lookupDefinition" parent="DocName-lookupDefinition-parentBean"/>

  <bean id="DocName-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition" p:title="Name Lookup">
    <property name="defaultSort">
      <bean parent="SortDefinition">
        <property name="attributeNames">
          <list>
            <value>nameTypeCode</value>
          </list>
        </property>
        <property name="sortAscending" value="true"/>
      </bean>
    </property>
    <property name="lookupFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="nameTypeCode"/>
        <bean parent="FieldDefinition" p:attributeName="title"/>
        <bean parent="FieldDefinition" p:attributeName="firstName"/>
        <bean parent="FieldDefinition" p:attributeName="lastName"/>
        <bean parent="FieldDefinition" p:attributeName="suffix"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="nameTypeCode"/>
        <bean parent="FieldDefinition" p:attributeName="title"/>
        <bean parent="FieldDefinition" p:attributeName="firstName"/>
        <bean parent="FieldDefinition" p:attributeName="lastName"/>
        <bean parent="FieldDefinition" p:attributeName="suffix"/>
      </list>
    </property>
  </bean>

</beans>
