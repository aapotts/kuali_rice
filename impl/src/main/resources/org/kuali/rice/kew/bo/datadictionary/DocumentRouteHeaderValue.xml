  <!-- Business Object Lookup Definition -->

  <bean id="DocumentRouteHeaderValue-lookupDefinition" parent="DocumentRouteHeaderValue-lookupDefinition-parentBean" />

  <bean id="DocumentRouteHeaderValue-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="lookupableID" value="docRouteHeaderValueLookupable" />
    
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition" p:attributeName="documentType.name" p:noDirectInquiry="true"/>      
        <!-- <bean parent="FieldDefinition"
              p:attributeName="initiatorWorkflowId" />
        
        <bean parent="FieldDefinition"
              p:attributeName="initiatorUser.principalName.id" />
        -->
        <bean parent="FieldDefinition" p:attributeName="documentId"/>
        <bean parent="FieldDefinition" p:attributeName="createDate"/>
     </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentId" />
        <bean parent="FieldDefinition"
              p:attributeName="documentType.name" />
        <bean parent="FieldDefinition"
              p:attributeName="docTitle" />
        <bean parent="FieldDefinition"
              p:attributeName="docRouteStatusLabel" />           
        <bean parent="FieldDefinition"
              p:attributeName="appDocStatus" />           
        <bean parent="FieldDefinition"
              p:attributeName="createDate" />
      </list>
    </property>
    <property name="title" value="Document Lookup" />
  </bean>
