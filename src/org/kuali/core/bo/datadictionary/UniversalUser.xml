<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
	<businessObject businessObjectClass="org.kuali.core.bo.user.UniversalUser">

		<inquiry>
			<title>Universal User Inquiry</title>

			<inquirySections>
				<inquirySection title="Header Text">
					<inquiryFields>
						<field attributeName="personUserIdentifier" />
						<field attributeName="personUniversalIdentifier" noInquiry="true" />
						<field attributeName="personPayrollIdentifier" />
						<field attributeName="personName" />
						<field attributeName="campusCode" />
						<field attributeName="primaryDepartmentCode" />
						<field attributeName="staff" />
						<field attributeName="faculty" />
						<field attributeName="student" />
						<field attributeName="affiliate" />
						<field attributeName="employeeStatusCode" />
						<field attributeName="employeeTypeCode" />
						<field attributeName="personEmailAddress" />
						<field attributeName="activeModuleCodeString" />
					</inquiryFields>
				</inquirySection>
			</inquirySections>
		</inquiry>

		<lookup>
			<lookupableID>universalUserLookupable</lookupableID>

			<title>User Lookup</title>
			<instructions>Lookup a User</instructions>

			<defaultSort attributeName="personUserIdentifier" sortAscending="true" />

			<lookupFields>
				<lookupField attributeName="personUserIdentifier" required="false" />
				<lookupField attributeName="personUniversalIdentifier" required="false" />
				<lookupField attributeName="personPayrollIdentifier" required="false" />
				<lookupField attributeName="personName" required="false" />
				<lookupField attributeName="personLastName" required="false" />
				<lookupField attributeName="personFirstName" required="false" />
				<lookupField attributeName="personEmailAddress" required="false" />
				<lookupField attributeName="campusCode" required="false" />
				<lookupField attributeName="primaryDepartmentCode" required="false" />
				<lookupField attributeName="staff" />
				<lookupField attributeName="faculty" />
				<lookupField attributeName="student" />
				<lookupField attributeName="affiliate" />
				<lookupField attributeName="employeeStatusCode" required="false">
					<defaultValue>A</defaultValue>
				</lookupField>
				<lookupField attributeName="employeeTypeCode" required="false">
					<defaultValue>P</defaultValue>
				</lookupField>
				<lookupField attributeName="activeModuleCodeString" required="false" />
			</lookupFields>

			<resultFields>
				<field attributeName="personUserIdentifier" />
				<field attributeName="personUniversalIdentifier" />
				<field attributeName="personPayrollIdentifier" />
				<field attributeName="personName" />
				<field attributeName="campusCode" />
				<field attributeName="primaryDepartmentCode" />
				<field attributeName="employeeStatusCode" />
				<field attributeName="employeeTypeCode" />
				<field attributeName="activeModuleCodeString" />
			</resultFields>
		</lookup>

		<titleAttribute>personUniversalIdentifier</titleAttribute>

		<attributes>
			<attribute name="personUniversalIdentifier" forceUppercase="true">
				<label>Universal User ID</label>
				<shortLabel>Univ User Id</shortLabel>

				<maxLength>10</maxLength>
				<validationPattern>
					<numeric />
				</validationPattern>
				<required>true</required>

				<control>
					<text size="10" />
				</control>

				<summary>Universal User ID</summary>
				<description>Universal User ID</description>
			</attribute>

			<attribute name="personUserIdentifier" forceUppercase="true">
				<label>User ID</label>
				<shortLabel>User ID</shortLabel>

				<maxLength>8</maxLength>
				<validationPattern>
					<anyCharacter />
				</validationPattern>
				<required>true</required>

				<control>
					<text size="8" />
				</control>

				<summary>User ID</summary>
				<description>User ID</description>
			</attribute>

			<attribute name="personPayrollIdentifier" forceUppercase="true">
				<label>Employee ID</label>
				<shortLabel>Empl ID</shortLabel>

				<maxLength>11</maxLength>
				<validationPattern>
					<numeric />
				</validationPattern>
				<required>false</required>

				<control>
					<text size="13" />
				</control>

				<summary>Empl ID</summary>
				<description>
					This field lists the individualâ€™s HRMS Employee ID.  
					This is a unique 9-digit number assigned to each 
					employee as they are entered into the HRMS system.
				</description>
			</attribute>

			<attribute name="personName" forceUppercase="true">
				<label>Display Name</label>
				<shortLabel>Name</shortLabel>

				<maxLength>80</maxLength>

				<control>
					<hidden/>
					<!-- enable if you want to be able to set the display name separate from the individual name fields
					<text size="40" />
					-->
				</control>

				<summary>Display Name</summary>
				<description>Name</description>

			</attribute>

			<attributeReference name="employeeStatusCode" sourceClassName="org.kuali.core.bo.EmployeeStatus" sourceAttributeName="code">
				<required>false</required>
				<control>
					<select valuesFinderClass="org.kuali.core.lookup.keyvalues.EmployeeStatusValuesFinder" /> 
				</control>
			</attributeReference>

			<attributeReference name="employeeTypeCode" sourceClassName="org.kuali.core.bo.EmployeeType" sourceAttributeName="code">
				<required>false</required>
				<control>
					<select valuesFinderClass="org.kuali.core.lookup.keyvalues.EmployeeTypeValuesFinder" />
				</control>
			</attributeReference>

			<attributeReference name="student" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericBoolean">
				<label>Student?</label>
				<shortLabel>Stdt?</shortLabel>
				<summary>Student?</summary>
				<description>Is this person a student?</description>
			</attributeReference>

			<attributeReference name="staff" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericBoolean">
				<label>Staff?</label>
				<shortLabel>Stf?</shortLabel>
				<summary>Staff?</summary>
				<description>Is this person a staff member?</description>
			</attributeReference>

			<attributeReference name="faculty" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericBoolean">
				<label>Faculty?</label>
				<shortLabel>Fcty?</shortLabel>
				<summary>Faculty?</summary>
				<description>Is this person a faculty member?</description>
			</attributeReference>

			<attributeReference name="affiliate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericBoolean">
				<label>Affiliate?</label>
				<shortLabel>Aflt?</shortLabel>
				<summary>Affiliate?</summary>
				<description>Is this person an affiliate of the institution?</description>
			</attributeReference>


			<attribute name="primaryDepartmentCode" forceUppercase="true">
				<label>Department Id</label>
				<shortLabel>DeptId</shortLabel>

				<maxLength>10</maxLength>
				<validationPattern>
					<anyCharacter />
				</validationPattern>

				<control>
					<text size="10" />
				</control>

				<summary>Chart-Org</summary>
				<description>Chart-Org</description>

			</attribute>

			<attribute name="personEmailAddress" forceUppercase="true">
				<label>Email Address</label>
				<shortLabel>Email</shortLabel>

				<maxLength>40</maxLength>
				<validationPattern>
					<emailAddress />
				</validationPattern>

				<control>
					<text size="40" />
				</control>

				<summary>Email Address</summary>
				<description>Email Address</description>

			</attribute>

			<attribute name="personFirstName" forceUppercase="true">
				<label>First Name</label>
				<shortLabel>1st Name</shortLabel>

				<maxLength>20</maxLength>
				<validationPattern>
					<anyCharacter allowWhitespace="true" />
				</validationPattern>
				<required>true</required>

				<control>
					<text size="20" />
				</control>

				<summary>Person First Name</summary>
				<description>Person First Name</description>

			</attribute>

			<attribute name="personLastName" forceUppercase="true">
				<label>Last Name</label>
				<shortLabel>Lst Name</shortLabel>

				<maxLength>20</maxLength>
				<validationPattern>
					<anyCharacter allowWhitespace="true" />
				</validationPattern>
				<required>true</required>

				<control>
					<text size="20" />
				</control>

				<summary>Person Last Name</summary>
				<description>Person Last Name</description>

			</attribute>

			<attribute name="personMiddleName" forceUppercase="true">
				<label>Middle Name</label>
				<shortLabel>Mdl Name</shortLabel>

				<maxLength>20</maxLength>
				<validationPattern>
					<anyCharacter allowWhitespace="true" />
				</validationPattern>
				<required>false</required>

				<control>
					<text size="20" />
				</control>

				<summary>Person Last Name</summary>
				<description>Person Last Name</description>

			</attribute>

			<attribute name="personCampusAddress" forceUppercase="true">
				<label>Campus Address</label>
				<shortLabel>Cmp Addr</shortLabel>

				<maxLength>55</maxLength>

				<control>
					<text size="40" />
				</control>

				<summary>Person Campus Address</summary>
				<description>Person Campus Address</description>

			</attribute>

			<attribute name="personLocalPhoneNumber" forceUppercase="false">
				<label>Local Phone Number</label>
				<shortLabel>Phn Nbr</shortLabel>

				<maxLength>10</maxLength>
				<validationPattern>
					<anyCharacter />
				</validationPattern>

				<control>
					<text size="10" />
				</control>

				<summary>Person Local Phone Number</summary>
				<description>Person Local Phone Number</description>

			</attribute>

			<attribute name="personBaseSalaryAmount" forceUppercase="false">
				<label>Base Salary</label>
				<shortLabel>Salary</shortLabel>

				<maxLength>19</maxLength>
				<validationPattern>
					<numeric />
				</validationPattern>

				<control>
					<text size="10" />
				</control>

				<summary>Person Base Salary</summary>
				<description>Person Base Salary</description>

			</attribute>

			<attribute name="personTaxIdentifier" forceUppercase="false">
				<label>Tax Identifier</label>
				<shortLabel>Tax ID</shortLabel>

				<maxLength>9</maxLength>
				<validationPattern>
					<numeric />
				</validationPattern>

				<control>
					<text size="10" />
				</control>

				<attributeAuthorization>
					<displayWorkgroup>UNIVERSAL_USER_MAINTAINERS</displayWorkgroup>
					<displayMask>
						<maskLiteral literal="***************" />
					</displayMask>
				</attributeAuthorization>


				<summary>SSN</summary>
				<description>Social Security Number</description>

			</attribute>

			<attribute name="personTaxIdentifierTypeCode" forceUppercase="true">
				<label>Tax Identifier Type</label>
				<shortLabel>Tax ID Type</shortLabel>

				<maxLength>1</maxLength>
				<validationPattern>
					<alphaNumeric />
				</validationPattern>

				<control>
					<select valuesFinderClass="org.kuali.core.lookup.keyvalues.PersonTaxIdentifierTypeCodeValuesFinder"/>
				</control>

				<summary>Tax Identifier Type</summary>
				<description>Tax Identifier Type</description>

			</attribute>

			<attribute name="financialSystemsEncryptedPasswordText" forceUppercase="false">
				<label>Password</label>
				<shortLabel>Password</shortLabel>

				<maxLength>200</maxLength>
				<validationPattern>
					<anyCharacter />
				</validationPattern>

				<control>
					<text size="20" />
				</control>

				<attributeAuthorization>
					<displayWorkgroup>UNIVERSAL_USER_MAINTAINERS</displayWorkgroup>
					<displayMask>
						<maskLiteral literal="***************" />
					</displayMask>
				</attributeAuthorization>


				<summary>Password</summary>
				<description>Password</description>

			</attribute>


			<attributeReference name="campusCode" sourceClassName="org.kuali.core.bo.Campus" sourceAttributeName="campusCode">
				<required>false</required>

				<control>
					<text size="2" />
				</control>
				<description>The code for the Campus of the user.</description>

			</attributeReference>

			<attributeReference name="activeForAnyModule" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator">
				<label>Active Kuali User</label>
				<shortLabel>ActiveKualiUser</shortLabel>
				<summary>
					<![CDATA[&nbsp;]]>
				</summary>
				<description>Indicates whether or not this UniversalUser is also an active user for any module of the application.</description>
			</attributeReference>

			<attribute name="activeModuleCodeString">
				<label>Active Module List</label>
				<shortLabel>Modules</shortLabel>

				<maxLength>200</maxLength>
				<validationPattern>
					<anyCharacter allowWhitespace="true" />
				</validationPattern>

				<control>
					<select valuesFinderClass="org.kuali.core.lookup.keyvalues.InstalledModulesValuesFinder" />
				</control>

				<summary>List of module codes for which this person is an active user.</summary>
				<description>A comma-delimited list representing the modules to which this UniversalUser has access.</description>

			</attribute>

			<attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />


		</attributes>
	</businessObject>
</dictionaryEntry>