<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "https://test.kuali.org/dtds/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="edu.sampleu.kim.bo.Application">

        <inquiry>
            <title>Application Inquiry</title>

            <inquirySections>
                <inquirySection title="Application">
                    <inquiryFields>
                        <field attributeName="id" forceInquiry="true" />
                        <field attributeName="name" forceInquiry="true" />
                        <field attributeName="description" forceInquiry="true" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>
        </inquiry>

        <lookup>
            <title>Application Lookup</title>
            <instructions>Look up a Application</instructions>
            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="userName" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="id" required="false" />
                <lookupField attributeName="userName" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="id" forceInquiry="true" />
                <field attributeName="userName" forceInquiry="true"/>
            </resultFields>
        </lookup>
        
        <titleAttribute>id</titleAttribute>
 
        <attributes>

            <attribute name="id">
                <label>Id</label>
                <shortLabel>Id</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><numeric/></validationPattern>

                <control>
                    <text size="10" />
                </control>
                
                <summary>Id</summary>
                <description>Id</description>
			</attribute>

            <attribute name="name" forceUppercase="false">
                <label>Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>30</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="30" />
                </control>

                <summary>Fiscal Officer User Name</summary>
                <description>Fiscal Officer User Name</description>
			</attribute>
		</attributes>
    </businessObject>  
    
</dictionaryEntry>