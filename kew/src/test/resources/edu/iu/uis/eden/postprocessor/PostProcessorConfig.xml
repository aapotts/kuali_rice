<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
	<ruleTemplates xmlns="ns:workflow/RuleTemplate" xsi:schemaLocation="ns:workflow/DocumentType resource:RuleTemplate">
		<ruleTemplate>
			<name>TrivialTemplate</name>
			<description>Trivial Template</description>
		</ruleTemplate>
	</ruleTemplates>
	<documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
 		<documentType>
			<name>testModifyDocumentInPostProcessor</name>
			<label>testModifyDocumentInPostProcessor</label>
			<postProcessorName>edu.iu.uis.eden.postprocessor.PostProcessorTest$DocumentModifyingPostProcessor</postProcessorName>
			<superUserWorkgroupName>TestWorkgroup</superUserWorkgroupName>
			<blanketApproveWorkgroupName>TestWorkgroup</blanketApproveWorkgroupName>
			<defaultExceptionWorkgroupName>TestWorkgroup</defaultExceptionWorkgroupName>
			<docHandler>none</docHandler>
			<policies>
				<policy>
					<name>PRE_APPROVE</name>
					<value>false</value>
				</policy>
				<policy>
					<name>DEFAULT_APPROVE</name>
					<value>false</value>
				</policy>
			</policies>
			<routePaths>
				<routePath>
					<start name="AdHoc" nextNode="Simple1"/>
					<requests name="Simple1" nextNode="Simple2"/>
					<requests name="Simple2"/>
				</routePath>
			</routePaths>
			<routeNodes>
				<start name="AdHoc">
					<activationType>P</activationType>
				</start>
				<requests name="Simple1">
					<activationType>P</activationType>
					<ruleTemplate>TrivialTemplate</ruleTemplate>
				</requests>
				<requests name="Simple2">
					<activationType>P</activationType>
					<ruleTemplate>TrivialTemplate</ruleTemplate>
				</requests>
			</routeNodes>
		</documentType>
	</documentTypes>

</data>