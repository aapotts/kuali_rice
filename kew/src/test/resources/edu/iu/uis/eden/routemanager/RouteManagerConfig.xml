<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
	<workgroups xmlns="ns:workflow/Workgroup" xsi:schemaLocation="ns:workflow/Workgroup resource:Workgroup">
		<workgroup>
			<id>100</id>
			<workgroupName>ExceptionRoutingWorkgroup</workgroupName>
			<description>ExceptionRoutingWorkgroup</description>
			<members>
				<authenticationId>user3</authenticationId>
			</members>
		</workgroup>
	</workgroups>
	<ruleAttributes xmlns="ns:workflow/RuleAttribute" xsi:schemaLocation="ns:workflow/RuleAttribute resource:RuleAttribute">
		<ruleAttribute>
			<name>ExplodingRuleAttribute</name>
			<className>edu.iu.uis.eden.routemanager.ExplodingRuleAttribute</className>
			<label>ExplodingRuleAttribute</label>
			<description>ExplodingRuleAttribute</description>
			<type>RuleAttribute</type>
		</ruleAttribute>
	</ruleAttributes>	
	<documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
		<!--  ExceptionRoutingTest  -->
		<documentType>
			<name>ExceptionRoutingSequentialDoc</name>
			<description>ExceptionRoutingSequentialDoc</description>
			<label>ExceptionRoutingSequentialDoc</label>
			<postProcessorName>edu.iu.uis.eden.postprocessor.DefaultPostProcessor</postProcessorName>
			<docHandler>_blank</docHandler>
			<superUserWorkgroupName>TestWorkgroup</superUserWorkgroupName>
			<blanketApproveWorkgroupName>TestWorkgroup</blanketApproveWorkgroupName>
			<defaultExceptionWorkgroupName>ExceptionRoutingWorkgroup</defaultExceptionWorkgroupName>
			<active>true</active>
			<policies>
				<policy>
					<name>PRE_APPROVE</name>
					<value>false</value>
				</policy>
				<policy>
					<name>DEFAULT_APPROVE</name>
					<value>false</value>
				</policy>
			</policies>
			<routePaths>
				<routePath>
					<start name="AdHoc" nextNode="RouteNode1" />
					<requests name="RouteNode1" />
				</routePath>
			</routePaths>
			<routeNodes>
				<start name="AdHoc">
					<activationType>P</activationType>
				</start>
				<requests name="RouteNode1">
					<activationType>P</activationType>
					<ruleTemplate>RouteNode1</ruleTemplate>
				</requests>
			</routeNodes>
		</documentType>
		<!-- Parallel Exception routing test -->
		<documentType>
			<name>ExceptionRoutingParallelDoc</name>
			<description>ExceptionRoutingParallelDoc</description>
			<label>ExceptionRoutingSequentialDoc</label>
			<postProcessorName>edu.iu.uis.eden.postprocessor.DefaultPostProcessor</postProcessorName>
			<docHandler>_blank</docHandler>
			<superUserWorkgroupName>TestWorkgroup</superUserWorkgroupName>
			<blanketApproveWorkgroupName>TestWorkgroup</blanketApproveWorkgroupName>
			<active>true</active>
			<policies>
				<policy>
					<name>PRE_APPROVE</name>
					<value>false</value>
				</policy>
				<policy>
					<name>DEFAULT_APPROVE</name>
					<value>false</value>
				</policy>
			</policies>
			<routePaths>
				<routePath>
					<start name="AdHoc" nextNode="Split" />
					<split name="Split" nextNode="RouteNode4">
						<branch name="Branch 1">
							<requests name="RouteNode1" nextNode="Explode" />
							<requests name="Explode" nextNode="Join" />
						</branch>
						<branch name="Branch 2">
							<requests name="RouteNode3" nextNode="Join" />
						</branch>
						<join name="Join" />
					</split>
					<requests name="RouteNode4" />
				</routePath>
			</routePaths>
			<routeNodes>
				<start name="AdHoc">
					<exceptionWorkgroupName>TestWorkgroup</exceptionWorkgroupName>
					<activationType>P</activationType>
				</start>
				<split name="Split">
					<exceptionWorkgroupName>TestWorkgroup</exceptionWorkgroupName>
				</split>
				<join name="Join">
					<exceptionWorkgroupName>TestWorkgroup</exceptionWorkgroupName>
				</join>
				<requests name="RouteNode1">
					<exceptionWorkgroupName>TestWorkgroup</exceptionWorkgroupName>
					<activationType>P</activationType>
					<ruleTemplate>RouteNode1</ruleTemplate>
				</requests>
				<requests name="Explode">
					<exceptionWorkgroupName>ExceptionRoutingWorkgroup</exceptionWorkgroupName>
					<activationType>P</activationType>
					<ruleTemplate>Explode</ruleTemplate>
				</requests>
				<requests name="RouteNode3">
					<exceptionWorkgroupName>TestWorkgroup</exceptionWorkgroupName>
					<activationType>P</activationType>
					<ruleTemplate>RouteNode3</ruleTemplate>
				</requests>
				<requests name="RouteNode4">
					<exceptionWorkgroupName>TestWorkgroup</exceptionWorkgroupName>
					<activationType>P</activationType>
					<ruleTemplate>RouteNode4</ruleTemplate>
				</requests>
			</routeNodes>
		</documentType>
		
		<!-- adhoc transition test document -->
		
		<documentType>
			<name>AdhocTransitionTestDocument</name>
			<description>AdhocTransitionTestDocument</description>
			<label>AdhocTransitionTestDocument</label>
			<postProcessorName>edu.iu.uis.eden.routemanager.ExceptionRoutingTestPostProcessor</postProcessorName>
			<docHandler>_blank</docHandler>
			<superUserWorkgroupName>TestWorkgroup</superUserWorkgroupName>
			<blanketApproveWorkgroupName>TestWorkgroup</blanketApproveWorkgroupName>
			<active>true</active>
			<policies>
				<policy>
					<name>PRE_APPROVE</name>
					<value>false</value>
				</policy>
				<policy>
					<name>DEFAULT_APPROVE</name>
					<value>false</value>
				</policy>
			</policies>
			<routePaths>
				<routePath>
					<start name="AdHoc" nextNode="requests" />
					<requests name="requests" />
				</routePath>
			</routePaths>
			<routeNodes>
				<start name="AdHoc">
					<exceptionWorkgroupName>TestWorkgroup</exceptionWorkgroupName>
					<activationType>P</activationType>
				</start>
				<requests name="requests">
					<exceptionWorkgroupName>TestWorkgroup</exceptionWorkgroupName>
					<activationType>P</activationType>
					<ruleTemplate>RouteNode4</ruleTemplate>
				</requests>
			</routeNodes>
		</documentType>
		<!-- end adhoc transition test document -->
		
	</documentTypes>
	<ruleTemplates xmlns="ns:workflow/RuleTemplate" xsi:schemaLocation="ns:workflow/RuleTemplate resource:RuleTemplate">
		<!--  ExceptionRoutingTest  -->
		<ruleTemplate>
			<name>RouteNode1</name>
			<description>RouteNode1</description>
		</ruleTemplate>
		<!--  Parallel Exception routing test -->
		<ruleTemplate>
			<name>Explode</name>
			<description>Exploder</description>
			<attributes>
				<attribute>
					<name>ExplodingRuleAttribute</name>
					<required>true</required>
				</attribute>
			</attributes>
		</ruleTemplate>
		<ruleTemplate>
			<name>RouteNode3</name>
			<description>RouteNode1</description>
		</ruleTemplate>
		<ruleTemplate>
			<name>RouteNode4</name>
			<description>RouteNode4</description>
		</ruleTemplate>
	</ruleTemplates>
	<rules xmlns="ns:workflow/Rule" xsi:schemaLocation="ns:workflow/Rule resource:Rule">
		<!--  Parallel Exception routing test -->
		<rule>
			<documentType>ExceptionRoutingParallelDoc</documentType>
			<ruleTemplate>RouteNode1</ruleTemplate>
			<description>RouteNode1</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<user>bmcgough</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>ExceptionRoutingParallelDoc</documentType>
			<ruleTemplate>RouteNode3</ruleTemplate>
			<description>RouteNode3</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<user>ewestfal</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>ExceptionRoutingParallelDoc</documentType>
			<ruleTemplate>Explode</ruleTemplate>
			<description>Explode</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<user>rkirkend</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>ExceptionRoutingParallelDoc</documentType>
			<ruleTemplate>RouteNode4</ruleTemplate>
			<description>RouteNode4</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<user>jhopf</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		
		<!--  -->
		<rule>
			<documentType>AdhocTransitionTestDocument</documentType>
			<ruleTemplate>RouteNode4</ruleTemplate>
			<description>RouteNode4</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<user>jhopf</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		
		<!--  -->
	</rules>
</data>