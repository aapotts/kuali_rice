					<user>bmcgough</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>SeqDocType</documentType>
			<ruleTemplate>Ack1Template</ruleTemplate>
			<description>Sequential Node Ack Rule 1</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<user>temay</user>
					<actionRequested>K</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>SeqDocType</documentType>
			<ruleTemplate>Ack2Template</ruleTemplate>
			<description>Sequential Node Ack Rule 2</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<user>jhopf</user>
					<actionRequested>K</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>ParallelDocType</documentType>
			<ruleTemplate>WorkflowDocumentTemplate</ruleTemplate>
			<description>Parallel Node Test Rule 1</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<user>bmcgough</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>ParallelDocType</documentType>
			<ruleTemplate>Ack1Template</ruleTemplate>
			<description>Parallel Node Ack Rule 1</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<user>temay</user>
					<actionRequested>K</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>ParallelDocType</documentType>
			<ruleTemplate>WorkflowDocument2Template</ruleTemplate>
			<description>Parallel Node Test Rule 2</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<user>rkirkend</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>ParallelDocType</documentType>
			<ruleTemplate>WorkflowDocument3Template</ruleTemplate>
			<description>Parallel Node Test Rule 3</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<user>pmckown</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>ParallelDocType</documentType>
			<ruleTemplate>WorkflowDocument4Template</ruleTemplate>
			<description>Parallel Node Test Rule 4</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<user>jitrue</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>ParallelDocType</documentType>
			<ruleTemplate>WorkflowDocument5Template</ruleTemplate>
			<description>Parallel Node Test Rule 5</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<user>shenl</user>
