					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>ParallelDocType</documentType>
			<ruleTemplate>WorkflowDocumentFinalTemplate</ruleTemplate>
			<description>Parallel Node Test Rule Final</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<user>xqi</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
	</rules>
</data>	
		