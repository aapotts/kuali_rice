<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
	<ruleTemplates xmlns="ns:workflow/RuleTemplate" xsi:schemaLocation="ns:workflow/RuleTemplate resource:RuleTemplate">
		<ruleTemplate allowOverwrite="true">
			<name>Template1</name>
			<description>Template1</description>
		</ruleTemplate>
		<ruleTemplate allowOverwrite="true">
			<name>Template2</name>
			<description>Template2</description>
		</ruleTemplate>
	</ruleTemplates>
	<documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
		<documentType>
			<name>UnitTestDocument</name>
			<description>UnitTestDocument</description>
			<label>UnitTestDocument</label>
			<postProcessorName>edu.iu.uis.eden.postprocessor.DefaultPostProcessor</postProcessorName>
			<superUserWorkgroupName>TestWorkgroup</superUserWorkgroupName>
			<blanketApproveWorkgroupName>TestWorkgroup</blanketApproveWorkgroupName>
			<defaultExceptionWorkgroupName>TestWorkgroup</defaultExceptionWorkgroupName>
			<docHandler>_blank</docHandler>
			<active>true</active>
			<policies>
				<policy>
					<name>PRE_APPROVE</name>
					<value>false</value>
				</policy>
				<policy>
					<name>DEFAULT_APPROVE</name>
					<value>true</value>
				</policy>
			</policies>
			<routePaths>
				<routePath>
					<start name="Initiated" nextNode="Template1" />
					<requests name="Template1" nextNode="Template2" />
					<requests name="Template2" />
				</routePath>
			</routePaths>
			<routeNodes>
				<start name="Initiated">
					<activationType>P</activationType>
				</start>
				<requests name="Template1">
					<activationType>S</activationType>
					<ruleTemplate>Template1</ruleTemplate>
				</requests>
				<requests name="Template2">
					<activationType>S</activationType>
					<ruleTemplate>Template2</ruleTemplate>
				</requests>
			</routeNodes>
		</documentType>
	</documentTypes>
	<rules xmlns="ns:workflow/Rule" xsi:schemaLocation="ns:workflow/Rule resource:Rule">
		<rule>
			<documentType>UnitTestDocument</documentType>
			<ruleTemplate>Template1</ruleTemplate>
			<description>Template1 Rule</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<user>ewestfal</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>UnitTestDocument</documentType>
			<ruleTemplate>Template2</ruleTemplate>
			<description>Template2 Rule</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<user>jhopf</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
	</rules>	
</data>