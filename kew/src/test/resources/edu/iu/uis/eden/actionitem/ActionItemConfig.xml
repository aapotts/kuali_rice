<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
	<workgroups xmlns="ns:workflow/Workgroup" xsi:schemaLocation="ns:workflow/Workgroup resource:Workgroup">
      <workgroup>
        <workgroupName>AIWG1</workgroupName>
        <description>AIWG1</description>
        <members>
          <authenticationId>ewestfal</authenticationId>
          <authenticationId>rkirkend</authenticationId>
        </members>
      </workgroup>
      <workgroup>
        <workgroupName>AIWG2</workgroupName>
        <description>AIWG2</description>
        <members>
          <authenticationId>jhopf</authenticationId>
          <authenticationId>bmcgough</authenticationId>
        </members>
      </workgroup>
      <workgroup>
        <workgroupName>AIWGNested1</workgroupName>
        <description>AIWGNested1</description>
        <members>
          <authenticationId>natjohns</authenticationId>
          <workgroupName>AIWG1</workgroupName>
        </members>
      </workgroup>
      <workgroup>
        <workgroupName>AIWGNested2</workgroupName>
        <description>AIWGNested1</description>
        <members>
          <authenticationId>pmckown</authenticationId>
          <workgroupName>AIWG2</workgroupName>
          <workgroupName>AIWGNested1</workgroupName>
        </members>
      </workgroup>
    </workgroups>
    <ruleTemplates xmlns="ns:workflow/RuleTemplate" xsi:schemaLocation="ns:workflow/RuleTemplate resource:RuleTemplate">
		<ruleTemplate>
			<name>RuleTemplate1</name>
			<description>RuleTemplate1</description>
		</ruleTemplate>
		<ruleTemplate>
			<name>RuleTemplate2</name>
			<description>RuleTemplate1</description>
		</ruleTemplate>
		<ruleTemplate>
			<name>RuleTemplate3</name>
			<description>RuleTemplate1</description>
		</ruleTemplate>
	</ruleTemplates>
	<documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
		<documentType>
			<name>ActionItemDocumentType</name>
			<description>TestDocumentType</description>
			<label>TestDocumentType</label>
			<postProcessorName>edu.iu.uis.eden.postprocessor.DefaultPostProcessor</postProcessorName>
			<superUserWorkgroupName>TestWorkgroup</superUserWorkgroupName>
			<docHandler>_blank</docHandler>
			<blanketApproveWorkgroupName>TestWorkgroup</blanketApproveWorkgroupName>
			<defaultExceptionWorkgroupName>TestWorkgroup</defaultExceptionWorkgroupName>
			<policies>
				<policy>
					<name>PRE_APPROVE</name>
					<value>false</value>
				</policy>
				<policy>
					<name>DEFAULT_APPROVE</name>
					<value>false</value>
				</policy>
			</policies>
			<routePaths>
				<routePath>
					<start name="AdHoc" nextNode="Split" />
					<split name="Split" nextNode="RuleTemplate3">
						<branch name="B1">
							<requests name="RuleTemplate1" nextNode="Join" />
						</branch>
						<branch name="B2">
							<requests name="RuleTemplate2" nextNode="Join" />
						</branch>
						<join name="Join" />
					</split>
					<requests name="RuleTemplate3" />
				</routePath>
			</routePaths>
			<routeNodes>
				<start name="AdHoc">
					<activationType>P</activationType>
				</start>
				<split name="Split" />
				<join name="Join" />
				<requests name="RuleTemplate1">
					<activationType>S</activationType>
					<ruleTemplate>RuleTemplate1</ruleTemplate>
				</requests>
				<requests name="RuleTemplate2">
					<activationType>S</activationType>
					<ruleTemplate>RuleTemplate2</ruleTemplate>
				</requests>
				<requests name="RuleTemplate3">
					<activationType>S</activationType>
					<ruleTemplate>RuleTemplate3</ruleTemplate>
				</requests>
			</routeNodes>
		</documentType>
		<documentType>
			<name>BAOrphanedRequestDocumentType</name>
			<label>Document Type used to test that Blanket Approval is not orphaning Action Requests</label>
			<postProcessorName>edu.iu.uis.eden.postprocessor.DefaultPostProcessor</postProcessorName>
			<superUserWorkgroupName>TestWorkgroup</superUserWorkgroupName>
			<docHandler>none</docHandler>
			<blanketApproveWorkgroupName>TestWorkgroup</blanketApproveWorkgroupName>
			<defaultExceptionWorkgroupName>TestWorkgroup</defaultExceptionWorkgroupName>
			<policies>
				<policy>
					<name>PRE_APPROVE</name>
					<value>false</value>
				</policy>
				<policy>
					<name>DEFAULT_APPROVE</name>
					<value>false</value>
				</policy>
			</policies>
			<routePaths>
				<routePath>
					<start name="AdHoc" nextNode="RuleTemplate1" />
					<requests name="RuleTemplate1" nextNode="RuleTemplate2" />
					<requests name="RuleTemplate2" nextNode="RuleTemplate3"/>
					<requests name="RuleTemplate3" />
				</routePath>
			</routePaths>
			<routeNodes>
				<start name="AdHoc">
					<activationType>P</activationType>
				</start>
				<requests name="RuleTemplate1">
					<activationType>S</activationType>
					<ruleTemplate>RuleTemplate1</ruleTemplate>
				</requests>
				<requests name="RuleTemplate2">
					<activationType>S</activationType>
					<ruleTemplate>RuleTemplate2</ruleTemplate>
				</requests>
				<requests name="RuleTemplate3">
					<activationType>S</activationType>
					<ruleTemplate>RuleTemplate3</ruleTemplate>
				</requests>
			</routeNodes>
		</documentType>
	</documentTypes>
	<rules xmlns="ns:workflow/Rule" xsi:schemaLocation="ns:workflow/Rule resource:Rule">
		<rule>
			<documentType>ActionItemDocumentType</documentType>
			<ruleTemplate>RuleTemplate1</ruleTemplate>
			<description>RuleTemplate1</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<workgroup>TestWorkgroup</workgroup>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>ActionItemDocumentType</documentType>
			<ruleTemplate>RuleTemplate2</ruleTemplate>
			<description>RuleTemplate2</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<workgroup>WorkflowAdmin</workgroup>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>

		<!-- Rules for the Orphaned request test -->
		<rule>
			<documentType>BAOrphanedRequestDocumentType</documentType>
			<ruleTemplate>RuleTemplate1</ruleTemplate>
			<description>RuleTemplate1</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<user>rkirkend</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>BAOrphanedRequestDocumentType</documentType>
			<ruleTemplate>RuleTemplate2</ruleTemplate>
			<description>RuleTemplate2</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<user>user1</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>BAOrphanedRequestDocumentType</documentType>
			<ruleTemplate>RuleTemplate3</ruleTemplate>
			<description>RuleTemplate3</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<workgroup>TestWorkgroup</workgroup>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
	</rules>
</data>