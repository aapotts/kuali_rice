			<type>RuleAttribute</type>
		</ruleAttribute>
		
	</ruleAttributes>
	<documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
		<documentType>
			<name>SeqDocType</name>
			<description>SeqDocType</description>
			<label>SeqDocType</label>
			<postProcessorName>edu.iu.uis.eden.postprocessor.DefaultPostProcessor</postProcessorName>
			<docHandler>_blank</docHandler>
			<superUserWorkgroupName>TestWorkgroup</superUserWorkgroupName>
			<blanketApproveWorkgroupName>TestWorkgroup</blanketApproveWorkgroupName>
			<active>true</active>
			<policies>
				<policy>
					<name>PRE_APPROVE</name>
					<value>false</value>
				</policy>
				<policy>
					<name>DEFAULT_APPROVE</name>
					<value>false</value>
				</policy>
			</policies>
			<routePaths>
				<routePath>
					<start name="AdHoc" nextNode="WorkflowDocument" />
					<requests name="WorkflowDocument" nextNode="Acknowledge1" />
					<requests name="Acknowledge1" nextNode="Acknowledge2" />
					<requests name="Acknowledge2" />
				</routePath>
			</routePaths>
			<routeNodes>
				<start name="AdHoc">
					<exceptionWorkgroupName>TestWorkgroup</exceptionWorkgroupName>
					<activationType>P</activationType>
				</start>
				<requests name="WorkflowDocument">
					<exceptionWorkgroupName>TestWorkgroup</exceptionWorkgroupName>
					<activationType>P</activationType>
					<ruleTemplate>WorkflowDocumentTemplate</ruleTemplate>
				</requests>
				<requests name="Acknowledge1">
					<exceptionWorkgroupName>TestWorkgroup</exceptionWorkgroupName>
					<activationType>P</activationType>
					<ruleTemplate>Ack1Template</ruleTemplate>
				</requests>
				<requests name="Acknowledge2">
					<exceptionWorkgroupName>TestWorkgroup</exceptionWorkgroupName>
					<activationType>P</activationType>
					<ruleTemplate>Ack2Template</ruleTemplate>
				</requests>
			</routeNodes>
		</documentType>
		<documentType>
			<name>ParallelDocType</name>
			<description>ParallelDocType</description>
			<label>ParallelDocType</label>
			<postProcessorName>edu.iu.uis.eden.postprocessor.DefaultPostProcessor</postProcessorName>
			<docHandler>_blank</docHandler>
			<superUserWorkgroupName>TestWorkgroup</superUserWorkgroupName>
			<blanketApproveWorkgroupName>TestWorkgroup</blanketApproveWorkgroupName>
			<defaultExceptionWorkgroupName>TestWorkgroup</defaultExceptionWorkgroupName>
			<active>true</active>
			<policies>
				<policy>
					<name>PRE_APPROVE</name>
					<value>false</value>
				</policy>
				<policy>
					<name>DEFAULT_APPROVE</name>
					<value>false</value>
				</policy>
			</policies>
			<routePaths>
				<routePath>
					<start name="AdHoc" nextNode="WorkflowDocument" />
					<requests name="WorkflowDocument" nextNode="Split" />
					<split name="Split" nextNode="WorkflowDocumentFinal">
						<branch name="Branch 1">
							<requests name="Acknowledge1" nextNode="WorkflowDocument2" />
							<requests name="WorkflowDocument2" nextNode="Join" />
						</branch>
						<branch name="Branch 2">
							<requests name="WorkflowDocument3" nextNode="Join" />
						</branch>
						<join name="Join" />
					</split>
					<requests name="WorkflowDocumentFinal" />
				</routePath>
			</routePaths>
			<routeNodes>
				<start name="AdHoc">
					<activationType>P</activationType>
				</start>
				<requests name="WorkflowDocument">
					<activationType>P</activationType>
					<ruleTemplate>WorkflowDocumentTemplate</ruleTemplate>
				</requests>
				<split name="Split" />
				<requests name="Acknowledge1">
					<activationType>P</activationType>
					<ruleTemplate>Ack1Template</ruleTemplate>
				</requests>
				<requests name="WorkflowDocument2">
					<activationType>P</activationType>
					<ruleTemplate>WorkflowDocument2Template</ruleTemplate>
				</requests>
				<requests name="WorkflowDocument3">
					<activationType>P</activationType>
					<ruleTemplate>WorkflowDocument3Template</ruleTemplate>
				</requests>
				<join name="Join" />
				<requests name="WorkflowDocumentFinal">
					<activationType>P</activationType>
					<ruleTemplate>WorkflowDocumentFinalTemplate</ruleTemplate>
				</requests>
			</routeNodes>
		</documentType>
		<documentType>
			<name>ParallelEmptyDocType</name>
			<description>ParallelDocType</description>
			<label>ParallelDocType</label>
			<postProcessorName>edu.iu.uis.eden.postprocessor.DefaultPostProcessor</postProcessorName>
			<docHandler>_blank</docHandler>
			<superUserWorkgroupName>TestWorkgroup</superUserWorkgroupName>
			<blanketApproveWorkgroupName>TestWorkgroup</blanketApproveWorkgroupName>
			<defaultExceptionWorkgroupName>TestWorkgroup</defaultExceptionWorkgroupName>
			<active>true</active>
			<policies>
				<policy>
					<name>PRE_APPROVE</name>
					<value>false</value>
				</policy>
				<policy>
					<name>DEFAULT_APPROVE</name>
					<value>false</value>
				</policy>
			</policies>
			<routePaths>
				<routePath>
					<start name="AdHoc" nextNode="WorkflowDocument" />
					<requests name="WorkflowDocument" nextNode="Split" />
					<split name="Split" nextNode="WorkflowDocumentFinal">
						<branch name="Branch 1">
							<simple name="EmptyNode1" nextNode="Join" />
						</branch>
						<branch name="Branch 2">
							<simple name="EmptyNode2" nextNode="Join" />
						</branch>
						<join name="Join" />
					</split>
					<requests name="WorkflowDocumentFinal" />
				</routePath>
			</routePaths>
			<routeNodes>
				<start name="AdHoc">
					<activationType>P</activationType>
				</start>
				<requests name="WorkflowDocument">
					<activationType>P</activationType>
					<ruleTemplate>WorkflowDocumentTemplate</ruleTemplate>
				</requests>
				<split name="Split" />
				<simple name="EmptyNode1">
					<activationType>P</activationType>
					<type>edu.iu.uis.eden.engine.node.InitialNode</type>
				</simple>
				<simple name="EmptyNode2">
					<activationType>P</activationType>
					<type>edu.iu.uis.eden.engine.AdHocApproversNode</type>
				</simple>
				<join name="Join" />
				<requests name="WorkflowDocumentFinal">
					<activationType>P</activationType>
					<ruleTemplate>WorkflowDocumentFinalTemplate</ruleTemplate>
				</requests>
			</routeNodes>
		</documentType>
		<documentType>
			<name>AdHocApproversDocType</name>
			<parent>ParallelDocType</parent>
			<description>AdHocApproversDocType</description>
			<label>AdHocApproversDocType</label>
			<postProcessorName>edu.iu.uis.eden.postprocessor.DefaultPostProcessor</postProcessorName>
			<docHandler>_blank</docHandler>
			<superUserWorkgroupName>TestWorkgroup</superUserWorkgroupName>
			<blanketApproveWorkgroupName>TestWorkgroup</blanketApproveWorkgroupName>
			<defaultExceptionWorkgroupName>TestWorkgroup</defaultExceptionWorkgroupName>
			<active>true</active>
			<policies>
				<policy>
					<name>PRE_APPROVE</name>
					<value>false</value>
				</policy>
				<policy>
					<name>DEFAULT_APPROVE</name>
					<value>false</value>
				</policy>
			</policies>
			<routePaths>
				<routePath>
					<start name="AdHoc" nextNode="WorkflowDocument" />
					<requests name="WorkflowDocument" nextNode="Split" />
					<split name="Split" nextNode="WorkflowDocumentFinal">
						<branch name="Branch 1">
							<requests name="WorkflowDocument2" nextNode="AdHocApproversJoin" />
						</branch>
						<branch name="Branch 2">
							<simple name="AdHocJoinPoint" nextNode="AdHocApproversJoin"/>
						</branch>
						<join name="AdHocApproversJoin" />
					</split>
					<requests name="WorkflowDocumentFinal" />
				</routePath>
			</routePaths>
			<routeNodes>
				<start name="AdHoc">
					<activationType>P</activationType>
				</start>
				<requests name="WorkflowDocument">
					<activationType>P</activationType>
					<ruleTemplate>WorkflowDocumentTemplate</ruleTemplate>
				</requests>
				<split name="Split" />
				<requests name="WorkflowDocument2">
					<activationType>P</activationType>
					<ruleTemplate>WorkflowDocument2Template</ruleTemplate>
				</requests>
				<simple name="AdHocJoinPoint">
					<activationType>P</activationType>
					<type>edu.iu.uis.eden.engine.node.NoOpNode</type>
				</simple>
				<join name="AdHocApproversJoin">
					<activationType>P</activationType>
					<ruleTemplate>WorkflowDocument3Template</ruleTemplate>
					<type>edu.iu.uis.eden.engine.AdHocApproversJoinNode</type>
				</join>
				<requests name="WorkflowDocumentFinal">
					<activationType>P</activationType>
					<ruleTemplate>WorkflowDocumentFinalTemplate</ruleTemplate>
				</requests>
			</routeNodes>
		</documentType>
		<documentType>
			<name>DynDocType</name>
			<description>DynDocType</description>
			<label>DynDocType</label>
			<postProcessorName>edu.iu.uis.eden.postprocessor.DefaultPostProcessor</postProcessorName>
			<docHandler>_blank</docHandler>
			<superUserWorkgroupName>TestWorkgroup</superUserWorkgroupName>
