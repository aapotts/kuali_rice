<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
	<ruleAttributes xmlns="ns:workflow/RuleAttribute" xsi:schemaLocation="ns:workflow/RuleAttribute resource:RuleAttribute">
		<ruleAttribute>
			<name>DynamicSubRoleAttribute</name>
			<className>edu.iu.uis.eden.engine.DynamicSubRoleAttribute</className>
			<label>Dynamic Sub Role Attribute</label>
			<description>Dynamic Sub Role Attribute</description>
			<type>RuleAttribute</type>
		</ruleAttribute>
		<!--  ChartOrgDynamicNodeTest  -->
		<ruleAttribute>
			<name>OrgRouting</name>
			<className>edu.iu.uis.eden.routing.chartorgnode.OrganizationRoutingAttribute</className>
			<label>Organization Routing</label>
			<description>Organization Routing</description>
			<type>RuleAttribute</type>
		</ruleAttribute>
		<ruleAttribute>
			<name>WorkgroupNameSearch</name>
			<className>edu.iu.uis.eden.workgroup.WorkgroupNameXmlAttribute</className>
			<label>WorkgroupName Search</label>
			<description>Searching by workgroup Name</description>
			<type>SearchableXmlAttribute</type>
			<searchingConfig>
				<fieldDef name="wrkgrp_nm" title="Workgroup Name">
					<display>
						<type>text</type>
					</display>
					<quickfinder service="WorkGroupLookupableImplService" appliesTo="workgroupName" draw="true" />
					<fieldEvaluation>
						<xpathexpression>//workgroup_name/value</xpathexpression>
					</fieldEvaluation>
				</fieldDef>
				<xmlSearchContent>
					<workgroup_name>
						<value>%wrkgrp_nm%</value>
					</workgroup_name>
				</xmlSearchContent>
			</searchingConfig>
		</ruleAttribute>
		<ruleAttribute>
			<name>ChartOrgDispatchAttribute</name>
			<className>edu.iu.uis.eden.engine.ChartOrgDispatchAttribute</className>
			<label>Organization Routing</label>
