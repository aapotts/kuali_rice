				<attribute>
					<name>DynamicSubRoleAttribute</name>
					<required>true</required>
				</attribute>
			</attributes>
		</ruleTemplate>
		<ruleTemplate>
			<name>Last</name>
			<description>Last</description>
		</ruleTemplate>
