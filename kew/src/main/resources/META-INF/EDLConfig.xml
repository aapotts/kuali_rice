<edlConfig>
	<preProcessors>
		<preProcessor>edu.iu.uis.eden.edl.components.WorkflowDocumentActions</preProcessor>
		<preProcessor>edu.iu.uis.eden.edl.components.VersioningPreprocessor</preProcessor>
	</preProcessors>
	<postProcessors>
		<postProcessor>edu.iu.uis.eden.edl.components.ValidationComponent</postProcessor>
        <postProcessor>edu.iu.uis.eden.edl.components.GlobalAttributeComponent</postProcessor>
		<postProcessor>edu.iu.uis.eden.edl.components.WorkflowDocumentActions</postProcessor>
		<postProcessor>edu.iu.uis.eden.edl.components.WorkflowDocumentState</postProcessor>
		<postProcessor>edu.iu.uis.eden.edl.components.NoteConfigComponent</postProcessor>
        <postProcessor>edu.iu.uis.eden.edl.components.AnnotationComponent</postProcessor>
	</postProcessors>
	<configProcessors>
		<configProcessor>
			<xpathExp>count(./@attributeName) = 1</xpathExp>
			<className>edu.iu.uis.eden.edl.components.AttributeEDLConfigComponent</className>
		</configProcessor>
		<configProcessor>
			<xpathExp>display/type = 'networkId'</xpathExp>
			<className>edu.iu.uis.eden.edl.components.NetworkIdWorkflowEDLConfigComponent</className>
		</configProcessor>
		<configProcessor>
			<xpathExp>display/type = 'universityId'</xpathExp>
			<className>edu.iu.uis.eden.edl.components.UniversityIdWorkflowEDLConfigComponent</className>
		</configProcessor>
		<configProcessor>
			<xpathExp>display/type = 'select' or display/type = 'text' or display/type = 'password' or display/type = 'radio' or display/type = 'checkbox' or display/type = 'textarea'</xpathExp>
			<className>edu.iu.uis.eden.edl.components.SimpleWorkflowEDLConfigComponent</className>
		</configProcessor>
		<configProcessor>
			<xpathExp>name() = 'instructions' or name() = 'createInstructions'</xpathExp>
			<className>edu.iu.uis.eden.edl.components.InstructionsEDLComponent</className>
		</configProcessor>
		<configProcessor>
			<xpathExp>name() = 'javascript'</xpathExp>
			<className>edu.iu.uis.eden.edl.components.JavascriptEDLComponent</className>
		</configProcessor>
	</configProcessors>

	<!--

	<stateComponents>
		<stateComponent>edu.iu.uis.eden.edl.TestStateComponent</stateComponent>
	</stateComponents>
	 -->
</edlConfig>
