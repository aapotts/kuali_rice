<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "https://test.kuali.org/dtds/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.core.bo.ParameterNamespace">

        <inquiry>
            <title>Parameter Namespace Inquiry</title>
            <inquirySections>
                <inquirySection title="Parameter Namespace Information">
                    <inquiryFields>
                        <field attributeName="code" />
                        <field attributeName="name" noInquiry="true" />
                        <field attributeName="active" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>
        </inquiry>
        <lookup>
            <title>Parameter Namespace Lookup</title>

            <instructions>Look up a Parameter Namespace.</instructions>
            <defaultSort attributeName="name" sortAscending="true" />
            <lookupFields>
                <lookupField attributeName="code" required="false"
                    noLookup="true"
                />
                <lookupField attributeName="name" required="false" />
                <lookupField attributeName="active" required="false">
                    <defaultValue>Y</defaultValue>
                </lookupField>
            </lookupFields>
            <resultFields>
                <field attributeName="code" forceInquiry="true" />
                <field attributeName="name" />
                <field attributeName="active" />
            </resultFields>
        </lookup>

        <titleAttribute>name</titleAttribute>

        <attributes>
            <attribute name="code" forceUppercase="true">
                <label>Parameter Namespace Code</label>
                <shortLabel>Nmspc Cd</shortLabel>
                <maxLength>20</maxLength>
                <validationPattern>
                    <anyCharacter />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="10" />
                </control>
                <summary>Code identifying the namespace.</summary>
                <description>
                    Code identifying the namespace.
                </description>
            </attribute>
            <attribute name="name" forceUppercase="false">
                <label>Parameter Namespace Name</label>
                <shortLabel>Nmspc Nm</shortLabel>
                <maxLength>40</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="40" />
                </control>
                <summary>The name of the namespace.</summary>
                <description>
                    The name of the namespace.
                </description>
            </attribute>
            <attribute name="codeAndDescription" forceUppercase="false">
                <label>Parameter Namespace</label>
                <shortLabel>Parm Nmspc</shortLabel>
                <maxLength>100</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>false</required>
                <control>
                    <text size="40" />
                </control>
                <summary>The name of the namespace.</summary>
                <description>
                    The name of the namespace.
                </description>
            </attribute>
            <attributeReference name="active"
                sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
                sourceAttributeName="activeIndicator"
            >
            </attributeReference>
            <attributeReference name="objectId"
                sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
                sourceAttributeName="objectId"
            />
            <attributeReference name="versionNumber"
                sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
                sourceAttributeName="versionNumber"
            />
        </attributes>
    </businessObject>
</dictionaryEntry>
