<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "https://test.kuali.org/dtds/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.core.bo.ParameterType">

        <inquiry>
            <title>Parameter Type Inquiry</title>
            <inquirySections>
                <inquirySection title="Parameter Type Information">
                    <inquiryFields>
                        <field attributeName="code" />
                        <field attributeName="name" />
                        <field attributeName="active" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>
        </inquiry>
        <lookup>
            <title>Parameter Type Lookup</title>

            <instructions>Look up a Parameter Type.</instructions>
            <defaultSort attributeName="name" sortAscending="true" />
            <lookupFields>
                <lookupField attributeName="code" required="false"
                    noLookup="true"
                />
                <lookupField attributeName="name" required="false" />
                <lookupField attributeName="active" required="false">
                    <defaultValue>Y</defaultValue>
                </lookupField>
            </lookupFields>
            <resultFields>
                <field attributeName="code" forceInquiry="true" />
                <field attributeName="name" />
                <field attributeName="active" />
            </resultFields>
        </lookup>

        <titleAttribute>name</titleAttribute>

        <attributes>
            <attribute name="code" forceUppercase="true">
                <label>Parameter Type Code</label>
                <shortLabel>Type Cd</shortLabel>
                <maxLength>5</maxLength>
                <validationPattern>
                    <alphaNumeric allowWhitespace="false" allowUnderscore="false" maxLength="5" />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="6" />
                </control>
                <summary>Code identifying the parameter type.</summary>
                <description>
                    Code identifying the parameter type.
                </description>
            </attribute>
            <attribute name="name" forceUppercase="false">
                <label>Parameter Type Name</label>
                <shortLabel>Type Nm</shortLabel>
                <maxLength>40</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="40" />
                </control>
                <summary>The name of the parameter type.</summary>
                <description>
                    The name of the parameter type.
                </description>
            </attribute>
            <attribute name="codeAndDescription" forceUppercase="false">
                <label>Parameter Type</label>
                <shortLabel>Parm Type</shortLabel>
                <maxLength>100</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>false</required>
                <control>
                    <text size="40" />
                </control>
                <summary>The name of the parameter type.</summary>
                <description>
                    The name of the parameter type.
                </description>
            </attribute>
            <attributeReference name="active"
                sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
                sourceAttributeName="activeIndicator"
            >
            </attributeReference>
            <attributeReference name="objectId"
                sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
                sourceAttributeName="objectId"
            />
            <attributeReference name="versionNumber"
                sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
                sourceAttributeName="versionNumber"
            />
        </attributes>
    </businessObject>
</dictionaryEntry>
