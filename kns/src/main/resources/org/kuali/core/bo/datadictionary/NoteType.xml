<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "https://test.kuali.org/dtds/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.core.bo.NoteType">


        <inquiry>
            <title>Note Type Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="noteTypeActiveIndicator" />
                <field attributeName="noteTypeCode" />
                <field attributeName="noteTypeDescription" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Note Type Lookup</title>

            <instructions>Look up a Note Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="noteTypeDescription" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="noteTypeActiveIndicator" required="false" />
                <lookupField attributeName="noteTypeCode" required="false" />
                <lookupField attributeName="noteTypeDescription" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="noteTypeActiveIndicator" />
                <field attributeName="noteTypeCode" />
                <field attributeName="noteTypeDescription" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="noteTypeActiveIndicator" forceUppercase="true">
                <label>Note Type Active Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>noteTypeActiveIndicator</summary>
                <description>noteTypeActiveIndicator description...</description>
			</attribute>

            <attribute name="noteTypeCode" forceUppercase="true">
                <label>Note Type Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>noteTypeCode</summary>
                <description>noteTypeCode description...</description>
			</attribute>

            <attribute name="noteTypeDescription" forceUppercase="true">
                <label>Note Type Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>100</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="102" />
                </control>

                <summary>noteTypeDescription</summary>
                <description>noteTypeDescription description...</description>
			</attribute>

            <attribute name="versionNumber" forceUppercase="true">
                <label>Version Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="10" />
                </control>

                <summary>versionNumber</summary>
                <description>versionNumber description...</description>
			</attribute>

        </attributes>
    </businessObject>
</dictionaryEntry>
