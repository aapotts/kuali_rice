                    <call-method-rule pattern="summary" methodname="setSummary" paramcount="0" />
                    <call-method-rule pattern="description" methodname="setDescription" paramcount="0" />

                    <call-method-rule pattern="formatterClass" methodname="setFormatterClass" paramcount="0" paramtypes="java.lang.Class" />

                    <set-next-rule methodname="addAttributeDefinition" />
                </pattern>

                <!--  attributeReference -->
                <pattern value="attributeReference">
                    <object-create-rule classname="org.kuali.core.datadictionary.AttributeReferenceDefinition" />
                    <set-properties-rule />

                    <call-method-rule pattern="name" methodname="setName" paramcount="0" />
