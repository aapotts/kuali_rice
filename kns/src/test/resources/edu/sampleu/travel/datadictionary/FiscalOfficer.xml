<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "https://test.kuali.org/dtds/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="edu.sampleu.travel.bo.FiscalOfficer">

        <inquiry>
            <title>Travel Fiscal Officer Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="id" forceInquiry="true" />
                <field attributeName="userName" forceInquiry="true" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Travel Fiscal Officer Lookup</title>

            <instructions>Look up a Travel Fiscal Officer</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="userName" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="id" required="false" />
                <lookupField attributeName="userName" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="id" forceInquiry="true" />
                <field attributeName="userName" forceInquiry="true"/>
            </resultFields>
        </lookup>
        
        <titleAttribute>id</titleAttribute>
 
        <attributes>

            <attribute name="id">
                <label>Travel Fiscal Officer Id</label>
                <shortLabel>Travel Fiscal Officer Id</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="10" />
                </control>
                
                <summary>Travel Fiscal Officer Id</summary>
                <description>Travel Fiscal Officer Id</description>
			</attribute>

            <attribute name="userName" forceUppercase="false">
                <label>Fiscal Officer User Name</label>
                <shortLabel>Fiscal Officer User Name</shortLabel>

                <maxLength>30</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="30" />
                </control>

                <summary>Fiscal Officer User Name</summary>
                <description>Fiscal Officer User Name</description>
			</attribute>
		</attributes>
    </businessObject>  
    
</dictionaryEntry>