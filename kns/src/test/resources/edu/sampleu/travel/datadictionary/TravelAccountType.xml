<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="edu.sampleu.travel.bo.TravelAccountType">

        <inquiry>
            <title>Travel Account Type Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="accountTypeCode" forceInquiry="true" />
                <field attributeName="name" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Travel Account Type Lookup</title>
			<menubar>
				<![CDATA[<a href="/SampleRiceClient/index.html">Main</a>]]>
			</menubar>
            <instructions>Look up a Travel Account Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="name" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="accountTypeCode" required="false" />
                <lookupField attributeName="name" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="accountTypeCode" />
                <field attributeName="name" />
            </resultFields>
        </lookup>
        
        <titleAttribute>accountTypeCode</titleAttribute>
 
        <attributes>

            <attribute name="accountTypeCode">
                <label>Travel Account Type Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <required>true</required>

                <control>
                    <text size="3" />
                </control>
                

                <summary>Travel Account Type Code</summary>
                <description>Travel Account Type Code</description>
			</attribute>

            <attribute name="name" forceUppercase="false">
                <label>Account Type Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>30</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <required>true</required>

                <control>
                    <text size="30" />
                </control>

                <summary>Account Type Name</summary>
                <description>Account Type Name</description>
			</attribute>

            <attribute name="codeAndDescription" forceUppercase="false">
                <label>Account Type</label>
                <shortLabel>Account Type</shortLabel>

                <maxLength>30</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <required>true</required>

                <control>
                    <text size="30" />
                </control>

                <summary>Account Type</summary>
                <description>Account Type</description>
			</attribute>
		</attributes>				
    </businessObject>
    
</dictionaryEntry>