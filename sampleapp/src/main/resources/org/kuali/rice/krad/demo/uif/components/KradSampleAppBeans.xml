  <!-- ComponentLibraryHome defs -->

  <bean id="ComponentLibraryHome" parent="KradSampleAppView">
    <property name="cssClasses">
      <list merge="true">
        <value>demo-componentLibView</value>
        <value>demo-componentLibHome</value>
      </list>
