<spring:beans xmlns="http://www.kuali.org/schema" xmlns:spring="http://www.springframework.org/schema/beans"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd http://www.kuali.org/schema http://www.kuali.org/krad/schema.xsd">
  <formView entryPageId="ConfigurationTestView-ProgressiveRender-Page"
            formClass="edu.sampleu.demo.kitchensink.UifComponentsTestForm" growlMessagingEnabled="true"
            headertext="Configuration Test View" id="ConfigurationTestView" namespaceCode="KR-NS" parent="Uif-FormView"
            viewHelperServiceClass="edu.sampleu.demo.kitchensink.UIfComponentsViewHelperServiceImpl">
    <navigation>
      <ref bean="ConfigurationTestView-Menu"/>
    </navigation>
    <items>
      <ref bean="ConfigurationTestView-ProgressiveRender-Page"/>
      <ref bean="ConfigurationTestView-Help-Page"/>
      <ref bean="ConfigurationTestView-Quickfinder-Page"/>
    </items>
    <theme>
      <viewTheme parent="Uif-KradTheme"/>
    </theme>
    <additionalCssFiles>
      <value>css/sampleapp.css</value>
    </additionalCssFiles>
    <help>
      <help externalHelpUrl="http://www.kuali.org" parent="Uif-Help" tooltipHelpContent="View help"/>
    </help>
  </formView>
  <formView id="ConfigurationTestView_KNS" parent="ConfigurationTestView">
    <theme>
      <viewTheme parent="Uif-ClassicKnsTheme"/>
    </theme>
  </formView>
  <navigationGroup id="ConfigurationTestView-Menu" parent="Uif-TabNavigationGroup">
    <items>
      <action actionLabel="Page 1" navigateToPageId="ConfigurationTestView-ProgressiveRender-Page"
              parent="Uif-NavigationActionLink"/>
      <action actionLabel="Help" navigateToPageId="ConfigurationTestView-Help-Page" parent="Uif-NavigationActionLink"/>
      <action actionLabel="Lookup Widgets" navigateToPageId="ConfigurationTestView-Quickfinder-Page"
              parent="Uif-NavigationActionLink"/>
    </items>
  </navigationGroup>
</spring:beans>
