<spring:beans xmlns="http://www.kuali.org/schema" xmlns:spring="http://www.springframework.org/schema/beans"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd http://www.kuali.org/schema http://www.kuali.org/krad/schema.xsd">
  <group disclosure.render="false" id="minMaxSection" parent="Uif-HorizontalBoxSection">
    <items>
      <inputField maxLength="5" minLength="2" parent="GenericTextField" propertyName="field9"/>
      <inputField exclusiveMin="2" inclusiveMax="50" parent="GenericTextField" propertyName="field10">
        <validCharactersConstraint>
          <numericPatternConstraint parent="NumericPatternConstraint"/>
        </validCharactersConstraint>
      </inputField>
    </items>
  </group>
  <group disclosure.render="false" id="requiredSection" parent="Uif-HorizontalBoxSection">
    <items>
      <inputField control.disabled="true" parent="GenericTextField" propertyName="field2" required="true"/>
      <inputField parent="GenericTextArea" propertyName="field3" required="true"/>
      <inputField parent="GenericRadio" propertyName="field4" required="true"/>
      <inputField fieldLabel.requiredMessagePlacement="RIGHT" parent="GenericSelection" propertyName="field5"
                  required="true"/>
    </items>
  </group>
  <group disclosure.render="false" id="preReqSection" parent="Uif-HorizontalBoxSection">
    <items>
      <inputField label="Field A" parent="GenericTextField" propertyName="field6">
        <dependencyConstraint>
          <prerequisiteConstraint parent="PrerequisiteConstraint" propertyName="field7"/>
          <prerequisiteConstraint parent="PrerequisiteConstraint" propertyName="field8"/>
        </dependencyConstraint>
      </inputField>
      <inputField label="Field B" parent="GenericTextField" propertyName="field7"/>
      <inputField label="Field C" parent="GenericTextField" propertyName="field8">
        <dependencyConstraint>
          <prerequisiteConstraint parent="PrerequisiteConstraint" propertyName="field6"/>
        </dependencyConstraint>
      </inputField>
    </items>
  </group>
  <group disclosure.render="false" id="mustOccursSection" parent="Uif-HorizontalBoxSection">
    <items>
      <ref bean="moGroup1"/>
      <ref bean="moGroup2"/>
      <ref bean="moGroup3"/>
    </items>
  </group>
  <group disclosure.render="false" id="moGroup1" parent="Uif-HorizontalBoxSection">
    <items>
      <inputField parent="GenericTextField" propertyName="field11"/>
      <inputField parent="GenericTextField" propertyName="field12"/>
      <inputField parent="GenericTextField" propertyName="field13"/>
      <inputField parent="GenericTextField" propertyName="field14">
        <mustOccurConstraints>
          <mustOccurConstraint max="2" min="1" parent="MustOccurConstraint">
            <prerequisiteConstraints>
              <prerequisiteConstraint parent="PrerequisiteConstraint" propertyName="field11"/>
            </prerequisiteConstraints>
            <mustOccurConstraints>
              <mustOccurConstraint max="2" min="2" parent="MustOccurConstraint">
                <prerequisiteConstraints>
                  <prerequisiteConstraint parent="PrerequisiteConstraint" propertyName="field12"/>
                  <prerequisiteConstraint parent="PrerequisiteConstraint" propertyName="field13"/>
                </prerequisiteConstraints>
              </mustOccurConstraint>
            </mustOccurConstraints>
          </mustOccurConstraint>
        </mustOccurConstraints>
      </inputField>
    </items>
  </group>
  <group disclosure.render="false" id="moGroup2" parent="Uif-HorizontalBoxSection">
    <items>
      <inputField parent="GenericTextField" propertyName="field15"/>
      <inputField parent="GenericTextField" propertyName="field16"/>
      <inputField parent="GenericTextField" propertyName="field17"/>
      <inputField parent="GenericTextField" propertyName="field18">
        <mustOccurConstraints>
          <mustOccurConstraint max="1" min="1" parent="MustOccurConstraint">
            <prerequisiteConstraints>
              <prerequisiteConstraint parent="PrerequisiteConstraint" propertyName="field15"/>
            </prerequisiteConstraints>
            <mustOccurConstraints>
              <mustOccurConstraint max="2" min="2" parent="MustOccurConstraint">
                <prerequisiteConstraints>
                  <prerequisiteConstraint parent="PrerequisiteConstraint" propertyName="field16"/>
                  <prerequisiteConstraint parent="PrerequisiteConstraint" propertyName="field17"/>
                </prerequisiteConstraints>
              </mustOccurConstraint>
            </mustOccurConstraints>
          </mustOccurConstraint>
        </mustOccurConstraints>
      </inputField>
    </items>
  </group>
  <group disclosure.render="false" id="moGroup3" parent="Uif-HorizontalBoxSection">
    <items>
      <inputField parent="GenericTextField" propertyName="field19"/>
      <inputField parent="GenericTextField" propertyName="field20"/>
      <inputField parent="GenericTextField" propertyName="field21"/>
      <inputField parent="GenericTextField" propertyName="field22"/>
      <inputField parent="GenericTextField" propertyName="field23">
        <mustOccurConstraints>
          <mustOccurConstraint max="2" min="1" parent="MustOccurConstraint">
            <mustOccurConstraints>
              <mustOccurConstraint max="2" min="1" parent="MustOccurConstraint">
                <prerequisiteConstraints>
                  <prerequisiteConstraint parent="PrerequisiteConstraint" propertyName="field19"/>
                  <prerequisiteConstraint parent="PrerequisiteConstraint" propertyName="field20"/>
                </prerequisiteConstraints>
              </mustOccurConstraint>
              <mustOccurConstraint max="2" min="2" parent="MustOccurConstraint">
                <prerequisiteConstraints>
                  <prerequisiteConstraint parent="PrerequisiteConstraint" propertyName="field21"/>
                  <prerequisiteConstraint parent="PrerequisiteConstraint" propertyName="field22"/>
                </prerequisiteConstraints>
              </mustOccurConstraint>
            </mustOccurConstraints>
          </mustOccurConstraint>
        </mustOccurConstraints>
      </inputField>
    </items>
  </group>
  <group disclosure.render="false" id="caseConstraintSection" layoutManager.orientation="VERTICAL"
         parent="Uif-VerticalBoxSection">
    <items>
      <inputField parent="Uif-InputField" propertyName="field24" width="auto">
        <control>
          <radioGroupControl id="caseConstraintSection-VerticalRadioControl" parent="Uif-VerticalRadioControl">
            <options>
              <spring:bean parent="Uif-KeyLabelPair">
                <spring:property name="key" value="case1"/>
              </spring:bean>
              <spring:bean parent="Uif-KeyLabelPair">
                <spring:property name="key" value="case2"/>
              </spring:bean>
              <spring:bean parent="Uif-KeyLabelPair">
                <spring:property name="key" value="case3"/>
              </spring:bean>
              <spring:bean parent="Uif-KeyLabelPair">
                <spring:property name="key" value="case4"/>
              </spring:bean>
            </options>
          </radioGroupControl>
        </control>
      </inputField>
      <ref bean="ccSection1"/>
      <ref bean="ccSection2"/>
      <ref bean="ccSection3"/>
      <ref bean="ccSection4"/>
    </items>
  </group>
  <group disclosure.render="false" id="ccSection1" layoutManager.orientation="VERTICAL" parent="Uif-VerticalBoxSection">
    <items>
      <inputField parent="GenericTextField" propertyName="field25">
        <caseConstraint>
          <caseConstriant parent="CaseConstraint" propertyName="field24">
            <whenConstraint>
              <whenConstraint parent="WhenConstraint">
                <values>
                  <value>case1</value>
                  <value>case2</value>
                </values>
                <constraint>
                  <simpleContraint parent="RequiredConstraint"/>
                </constraint>
              </whenConstraint>
              <whenConstraint parent="WhenConstraint" value="case2">
                <constraint>
                  <alphaNumericPatternConstraint parent="AlphaNumericPatternConstraint"/>
                </constraint>
              </whenConstraint>
            </whenConstraint>
          </caseConstriant>
        </caseConstraint>
      </inputField>
    </items>
  </group>
  <group disclosure.render="false" id="ccSection2" layoutManager.orientation="VERTICAL" parent="Uif-VerticalBoxSection">
    <items>
      <inputField parent="GenericTextField" propertyName="field26">
        <caseConstraint>
          <caseConstriant parent="CaseConstraint" propertyName="field24">
            <whenConstraint>
              <whenConstraint parent="WhenConstraint" value="case3">
                <constraint>
                  <simpleContraint inclusiveMax="500" maxLength="3" parent="SimpleConstraint"/>
                </constraint>
              </whenConstraint>
            </whenConstraint>
          </caseConstriant>
        </caseConstraint>
      </inputField>
    </items>
  </group>
  <group disclosure.render="false" id="ccSection3" parent="Uif-HorizontalBoxSection">
    <items>
      <inputField parent="GenericTextField" propertyName="field27">
        <caseConstraint>
          <caseConstriant parent="CaseConstraint" propertyName="field24">
            <whenConstraint>
              <whenConstraint parent="WhenConstraint" value="case4">
                <constraint>
                  <prerequisiteConstraint parent="PrerequisiteConstraint" propertyName="field28"/>
                </constraint>
              </whenConstraint>
            </whenConstraint>
          </caseConstriant>
        </caseConstraint>
      </inputField>
      <inputField parent="GenericTextField" propertyName="field28"/>
    </items>
  </group>
  <group disclosure.render="false" id="ccSection4" parent="Uif-HorizontalBoxSection">
    <items>
      <inputField parent="GenericTextField" propertyName="field31"/>
      <inputField parent="GenericTextField" propertyName="field32"/>
      <inputField parent="GenericTextField" propertyName="field33">
        <caseConstraint>
          <caseConstriant operator="has_value" parent="CaseConstraint" propertyName="field31">
            <whenConstraint>
              <whenConstraint parent="WhenConstraint" value="true">
                <constraint>
                  <caseConstriant operator="not_equals" parent="CaseConstraint" propertyName="field32">
                    <whenConstraint>
                      <whenConstraint parent="WhenConstraint" value="A">
                        <constraint>
                          <simpleContraint parent="RequiredConstraint"/>
                        </constraint>
                      </whenConstraint>
                    </whenConstraint>
                  </caseConstriant>
                </constraint>
              </whenConstraint>
            </whenConstraint>
          </caseConstriant>
        </caseConstraint>
      </inputField>
    </items>
  </group>
</spring:beans>
