<spring:beans xmlns:spring="http://www.springframework.org/schema/beans" xmlns="http://www.kuali.org/krad/schema"
              xmlns:p="http://www.springframework.org/schema/p" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd http://www.kuali.org/krad/schema http://www.kuali.org/krad/schema.xsd">
  <verticalBoxSection-bean disclosure.render="false" id="regex1" layoutManager.orientation="VERTICAL">
    <items>
      <inputField-bean parent="GenericTextField" propertyName="field50">
        <validCharactersConstraint>
          <fixedPointPatternConstraint-bean precision="5" scale="2"/>
        </validCharactersConstraint>
      </inputField-bean>
      <inputField-bean parent="GenericTextField" propertyName="field51">
        <control>
          <textControl-bean id="regex1-field51-textControl" size="30"/>
        </control>
        <validCharactersConstraint>
          <floatingPointPatternConstraint-bean allowNegative="true"/>
        </validCharactersConstraint>
      </inputField-bean>
      <inputField-bean parent="GenericTextField" propertyName="field77">
        <validCharactersConstraint>
          <integerPatternConstraint-bean allowNegative="true"/>
        </validCharactersConstraint>
      </inputField-bean>
      <inputField-bean parent="GenericTextField" propertyName="field52">
        <validCharactersConstraint>
          <phoneNumberPatternConstraint-bean/>
        </validCharactersConstraint>
      </inputField-bean>
      <inputField-bean parent="GenericTextField" propertyName="field53">
        <validCharactersConstraint>
          <javaClassPatternConstraint-bean/>
        </validCharactersConstraint>
      </inputField-bean>
      <inputField-bean parent="GenericTextField" propertyName="field54">
        <validCharactersConstraint>
          <emailAddressPatternConstraint-bean/>
        </validCharactersConstraint>
      </inputField-bean>
      <inputField-bean parent="GenericTextField" propertyName="field84">
        <validCharactersConstraint>
          <urlPatternConstraint-bean/>
        </validCharactersConstraint>
      </inputField-bean>
      <inputField-bean parent="GenericTextField" propertyName="field55">
        <validCharactersConstraint>
          <datePatternConstraint-bean/>
        </validCharactersConstraint>
      </inputField-bean>
      <inputField-bean parent="GenericTextField" propertyName="field75">
        <validCharactersConstraint>
          <basicDatePatternConstraint-bean/>
        </validCharactersConstraint>
      </inputField-bean>
      <inputField-bean parent="GenericTextField" propertyName="field82">
        <validCharactersConstraint>
          <timePatternConstraint-bean/>
        </validCharactersConstraint>
      </inputField-bean>
      <inputField-bean parent="GenericTextField" propertyName="field83">
        <validCharactersConstraint>
          <time24HPatternConstraint-bean/>
        </validCharactersConstraint>
      </inputField-bean>
      <inputField-bean parent="GenericTextField" propertyName="field56">
        <validCharactersConstraint>
          <timestampPatternConstraint-bean/>
        </validCharactersConstraint>
      </inputField-bean>
      <inputField-bean parent="GenericTextField" propertyName="field57">
        <validCharactersConstraint>
          <yearPatternConstraint-bean/>
        </validCharactersConstraint>
      </inputField-bean>
      <inputField-bean parent="GenericTextField" propertyName="field58">
        <validCharactersConstraint>
          <monthPatternConstraint-bean/>
        </validCharactersConstraint>
      </inputField-bean>
      <inputField-bean parent="GenericTextField" propertyName="field61">
        <validCharactersConstraint>
          <zipcodePatternConstraint-bean/>
        </validCharactersConstraint>
      </inputField-bean>
      <inputField-bean parent="GenericTextField" propertyName="field62">
        <validCharactersConstraint>
          <alphaNumericPatternConstraint-bean/>
        </validCharactersConstraint>
      </inputField-bean>
      <inputField-bean parent="GenericTextField" propertyName="field63">
        <validCharactersConstraint>
          <alphaNumericPatternConstraint-bean allowForwardSlash="true" allowUnderscore="true" allowWhitespace="true"/>
        </validCharactersConstraint>
      </inputField-bean>
      <inputField-bean parent="GenericTextField" propertyName="field64">
        <validCharactersConstraint>
          <alphaPatternConstraint-bean allowComma="true" allowWhitespace="true"/>
        </validCharactersConstraint>
      </inputField-bean>
      <inputField-bean parent="GenericTextField" propertyName="field76">
        <validCharactersConstraint>
          <alphaPatternConstraint-bean allowAll="true"/>
        </validCharactersConstraint>
      </inputField-bean>
      <inputField-bean parent="GenericTextField" propertyName="field65">
        <validCharactersConstraint>
          <noWhitespacePatternConstraint-bean/>
        </validCharactersConstraint>
      </inputField-bean>
      <inputField-bean parent="GenericTextField" propertyName="field66">
        <validCharactersConstraint>
          <charsetPatternConstraint-bean validChars="abcABC"/>
        </validCharactersConstraint>
      </inputField-bean>
      <inputField-bean parent="GenericTextField" propertyName="field67">
        <validCharactersConstraint>
          <numericPatternConstraint-bean allowDash="true" allowParenthesis="true"/>
        </validCharactersConstraint>
      </inputField-bean>
      <inputField-bean parent="GenericTextField" propertyName="field68">
        <validCharactersConstraint>
          <validCharactersConstraint-bean messageKey="validation.aDotNumTest" value="^[a-zA-Z]\.[0-9]$"/>
        </validCharactersConstraint>
      </inputField-bean>
    </items>
  </verticalBoxSection-bean>
</spring:beans>
