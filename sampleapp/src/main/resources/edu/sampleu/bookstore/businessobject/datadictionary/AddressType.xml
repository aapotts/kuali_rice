<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:p="http://www.springframework.org/schema/p" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
  <bean id="AddressType" parent="AddressType-parentBean"/>
  <bean id="AddressType-parentBean" abstract="true" parent="DataObjectEntry">
    <property name="dataObjectClass" value="edu.sampleu.bookstore.bo.AddressType"/>
    <property name="objectLabel" value="Address Type"/>
    <property name="attributes">
      <list>
        <ref bean="AddressType-type"/>
        <ref bean="AddressType-description"/>
        <ref bean="AddressType-activeBean"/>
      </list>
    </property>
    <property name="titleAttribute" value="type"/>
    <property name="primaryKeys">
      <list>
        <value>type</value>
      </list>
    </property>
  </bean>
  <bean id="AddressType-type" parent="AddressType-type-parentBean"/>
  <bean id="AddressType-type-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="type"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Address Type"/>
    <property name="shortLabel" value="Type"/>
    <property name="maxLength" value="40"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="false"/>
    </property>
    <property name="required" value="true"/>
    <property name="controlField">
      <bean parent="TextControl" p:size="30"/>
    </property>
    <property name="summary" value="Type identifying the Authr."/>
    <property name="description" value="Type identifying the Authr."/>
  </bean>
  <bean id="AddressType-description" parent="AddressType-description-parentBean"/>
  <bean id="AddressType-description-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="description"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Address Description"/>
    <property name="shortLabel" value="Description"/>
    <property name="maxLength" value="100"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true"/>
    </property>
    <property name="required" value="true"/>
    <property name="controlField">
      <bean parent="TextControl" p:size="50"/>
    </property>
    <property name="summary" value="Description for the address type."/>
    <property name="description" value="Description for the address type."/>
  </bean>
  <bean id="AddressType-activeBean" parent="AddressType-activeBean-parentBean"/>
  <bean id="AddressType-activeBean-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="active"/>
    <property name="forceUppercase" value="false"/>
    <property name="shortLabel" value="active"/>
    <property name="maxLength" value="1"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern"/>
    </property>
    <property name="controlField">
      <bean parent="CheckboxControl"/>
    </property>
    <property name="summary" value="Active Indicator"/>
    <property name="formatterClass" value="org.kuali.rice.core.web.format.BooleanFormatter"/>
    <property name="label" value="Active Indicator"/>
    <property name="description" value="This attribute is used to describe whether the associated object is active or inactive."/>
  </bean>
  <bean id="AddressType-InquiryView" parent="InquiryView">
    <property name="title" value="Address Type Inquiry"/>
    <property name="dataObjectClassName" value="edu.sampleu.bookstore.bo.AddressType"/>
    <property name="Items">
      <list>
        <bean parent="GroupSectionGridLayout">
          <property name="title" value="Address Type Information"/>
          <property name="layoutManager.numberOfColumns" value="2"/>
          <property name="items">
            <list>
              <bean parent="AttributeField" p:propertyName="type"/>
              <bean parent="AttributeField" p:propertyName="description"/>
              <bean parent="AttributeField" p:propertyName="active"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
  <bean id="AddressType-LookupView" parent="LookupView">
    <property name="title" value="Address Type Lookup"/>
    <property name="dataObjectClassName" value="edu.sampleu.bookstore.bo.AddressType"/>
    <property name="criteriaFields">
      <list>
        <bean parent="LookupCriteriaAttributeField" p:propertyName="type"/>
        <bean parent="LookupCriteriaAttributeField" p:propertyName="description"/>
        <bean parent="LookupCriteriaAttributeField" p:propertyName="active"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="AttributeField" p:propertyName="type"/>
        <bean parent="AttributeField" p:propertyName="description"/>
        <bean parent="AttributeField" p:propertyName="active"/>
      </list>
    </property>
  </bean>
</beans>
